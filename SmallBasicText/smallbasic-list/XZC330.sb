GraphicsWindow.MouseUp = Click

GraphicsWindow.ShowMessage("Watch direct Click() call","")

Click()

Program.Delay(1000)

GraphicsWindow.Clear()

GraphicsWindow.DrawText(100,250, "Now, click to watch event call of Click")



Sub Click

  GraphicsWindow.DrawText(100,100, "text")

  Program.Delay(1000)

  GraphicsWindow.DrawText(100,150, "text2")

  Program.Delay(1000)

  GraphicsWindow.DrawText(100,200, "text3")

EndSub


