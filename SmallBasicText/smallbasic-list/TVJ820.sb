'  Keys    S, W, A, T, X,Z,F , SpaceBar works 

Sub OnKeyDown

  key= GraphicsWindow.LastKey

  If key="S" Then

    ddx=4

  ElseIf Key="W"then

    ddx=-4

  ElseIf Key="A"then

    ddy=4

  ElseIf key="T"Then

    ddy=-4

  ElseIf key="Space" Then

    ddx=0     'WhT 

    ddy=0

  EndIf

  If Key = "X" Then

    Shapes.Rotate(cannon,angle+5)

    angle=angle+5

  ElseIf Key = "Z" Then

    Shapes.Rotate(cannon,angle-5)

    angle=angle-5

  ElseIf Key="F" then

    Shapes.Animate(ball,GraphicsWindow.width/2,sprite1_y-60,600)

    Shapes.ShowShape(Ball)

    Timer.Resume()

  EndIf

EndSub


