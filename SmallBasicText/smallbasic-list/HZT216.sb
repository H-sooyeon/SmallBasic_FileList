'Timers-------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

Sub HardcoreCounter

  Countdown = 60

  Timer.Tick = Seconds

  Timer.Interval = 1000

EndSub

Sub Seconds

  If Countdown > 0 Then

    Countdown = Countdown - 1

    GraphicsWindow.BrushColor = "#FFD700"

    GraphicsWindow.FillRectangle(1200, 0, 500, 100)

    GraphicsWindow.BrushColor = "Red"

    GraphicsWindow.FontSize = 40

    GraphicsWindow.DrawText(1300, 50, "" + Countdown + "")

    If Countdown = 57 Then

      HardcoreBallons()

    ElseIf Countdown = 55 And Wave > 7 Then

      HardcoreBallons()

    ElseIf Countdown = 51 And Wave > 2 Then

      HardcoreBallons()

    ElseIf Countdown = 45 Then

      HardcoreBallons()

    ElseIf Countdown = 42 And Wave > 4 Then 

      HardcoreBallons()

    ElseIf Countdown = 39 Then

      HardcoreBallons()

    ElseIf Countdown = 33 And Wave > 1 Then

      HardcoreBallons()

    ElseIf Countdown = 28 Then

      HardcoreBallons()

    ElseIf Countdown = 26 And Wave > 5 Then

      HardcoreBallons()

    ElseIf Countdown = 25 Then  

      HardcoreBallons()

    EndIf

  ElseIf Countdown = 0 Then

    GraphicsWindow.BrushColor = "Red"

    GraphicsWindow.DrawText(400, 300, "Wave " + Wave + " Complete")

    GraphicsWindow.DrawText(400, 400, "New Wave Incoming")

    Program.Delay(3000)

    GraphicsWindow.BrushColor = "#FFD700"

    GraphicsWindow.FillRectangle(390, 290, 500, 300)

    Countdown = 60

    Wave = Wave + 1

  EndIf 

EndSub

