'Spike Control-------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

Sub KeyPress

  Key = GraphicsWindow.LastKey

  If Key = "Left" Then

    TriangleX = TriangleX - 50

    Shapes.Move(Spike, TriangleX, TriangleY)

    Barrier()

  ElseIf Key = "Right" Then

    TriangleX = TriangleX + 50

    Shapes.Move(Spike, TriangleX, TriangleY)

    Barrier()

  ElseIf Key = "Up" And HardcoreGame = 1 Then

    TriangleY = TriangleY - 50

    Shapes.Move(Spike, TriangleX, TriangleY)

    Barrier()

  ElseIf Key = "Down" And HardcoreGame = 1 Then

    TriangleY = TriangleY + 50

    Shapes.Move(Spike, TriangleX, TriangleY)

    Barrier()

  ElseIf Key = "A" And CoopGame = 1 Then

    TriangleX2 = TriangleX2 - 50

    Shapes.Move(SecondSpike, TriangleX2, TriangleY2)

    Barrier()

  ElseIf Key = "D" And CoopGame = 1 Then

    TriangleX2 = TriangleX2 + 50

    Shapes.Move(SecondSpike, TriangleX2, TriangleY2)

    Barrier()

  ElseIf Key = "Space" And ShooterGame = 1 Then

    Shapes.Move(Bullet, (TriangleX + 100), 680)

    Shapes.ShowShape(Bullet)

    Shapes.Animate(Bullet, (TriangleX + 100), -40, 500)

  EndIf

EndSub

Sub Barrier

  If TriangleX > 1175 Then

    TriangleX = 1175

  ElseIf TriangleX < 50 Then

    TriangleX = 50

  ElseIf TriangleY > 700 And HardcoreGame = 1 Then

    TriangleY = 700

  ElseIf TriangleY < 100 And HardcoreGame = 1 Then

    TriangleY = 100

  ElseIf TriangleX2 > 1175 Then

    TriangleX2 = 1175

  ElseIf TriangleX2 < 50 Then

    TriangleX2 = 50

  EndIf

EndSub

