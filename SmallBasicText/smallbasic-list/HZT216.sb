'Lives--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

Sub LifeTracker

  If Lives = 0 Then

    Shapes.HideShape(Spike)

    Shapes.HideShape(SecondSpike)

    Shapes.HideShape(RedBalloon)

    Shapes.HideShape(BlueBalloon)

    Shapes.HideShape(GreenBalloon)

    Shapes.HideShape(PinkBalloon)

    Shapes.HideShape(CyanBalloon)

    Shapes.HideShape(OrangeBalloon)

    Countdown = -1

    GraphicsWindow.BrushColor = "#FFD700"

    GraphicsWindow.FillRectangle(0, 0, 1420, 825)

    GraphicsWindow.BrushColor = "Red"

    GraphicsWindow.FontSize = 50

    GraphicsWindow.DrawText(300, 300, "Game Over")

    GraphicsWindow.FontSize = 40

    GraphicsWindow.DrawText(300, 300, "Final Score:" + Score + "")

  EndIf

EndSub

