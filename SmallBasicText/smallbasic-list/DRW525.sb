TextWindow.WriteLine("type the character: ")
character = TextWindow.ReadKey()
TextWindow.WriteLine("character code " + Text.GetCharacterCode(character))

TextWindow.Pause()

For i =  0 To 127
  TextWindow.CursorLeft = 0
  TextWindow.Write(i)
  TextWindow.CursorLeft = 4
  TextWindow.WriteLine(Text.GetCharacter(i))
EndFor
