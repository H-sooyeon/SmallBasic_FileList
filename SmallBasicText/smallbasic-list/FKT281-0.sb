GraphicsWindow.Title="CMY mix leafs"
Init()
GraphicsWindow.LEFT=200
GraphicsWindow.BackgroundColor = "white
ll="#7f
For i = 1 To 2 
  x = ox + (i - 1.53) * 1.2 * 250 - size / 2
  y = oy - size / 2
  c= LDColours.HSLtoRGB ((i-1)*120+180 1 .5)
  'GraphicsWindow.Title=c
  c=ll+text.GetSubTextToEnd (c 2)
  GraphicsWindow.BrushColor =c
  GraphicsWindow.FillEllipse(x, y-35, size, size)
EndFor
 GraphicsWindow.BrushColor =ll+"FFFF00
i = 1  
  x = ox + (i - 1.53) * 1.2 * 250 - size / 2
  y = oy - size / 2
  GraphicsWindow.FillEllipse(x+160, y+200, size, size)


Sub Init
  gw = 500
  gh = 450
  GraphicsWindow.Width = gw
  GraphicsWindow.Height = gh
  ox = gw / 2
  oy = gh / 2
  size = 500
EndSub

