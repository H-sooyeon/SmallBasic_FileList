GraphicsWindow.BrushColor = "Wheat"

x = 400

y = 100

Size = 10

Angle  = Size / 4 

GraphicsWindow.DrawRectangle(x,y,Size,Size)

GraphicsWindow.DrawLine(x,y,x + Angle ,y - Angle )

GraphicsWindow.DrawLine(x + Angle ,y - Angle ,x + Size + Angle ,y - Angle )

GraphicsWindow.DrawLine(x + Size + Angle ,y - Angle ,x + Size,y)

GraphicsWindow.DrawLine(x + Size + Angle ,y - Angle ,x + Size + Angle ,y + Size - Angle )

GraphicsWindow.DrawLine(x + Size + Angle ,y + Size - Angle ,x + Size,y + Size)


