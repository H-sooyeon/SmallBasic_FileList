    if text.EndsWith (tt[x] "H") then 

      Turtle.PenUp ()

    endif

    Turtle.MoveTo (GraphicsWindow.Width/2 GraphicsWindow.Height/2  )

    Turtle.PenDown ()

    Turtle.Angle=0

