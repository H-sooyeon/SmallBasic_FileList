Sub rcc'------------------------------------------------------rightclickmenu-----------------------------------------
  lc= LDDialogs.LastRightClickMenuItem
  If lc=1 Then
    For x=1 To 59
      LDShapes.BrushShape("Polygon"+x LDShapes.BrushImage(fl[fa[x]] ))
    endfor
    
  elseIf lc=5 or lc=7 Then
    For x=1 To 59
      If Array.ContainsValue(eur x) then 
        If lc=7 and (x=37 or x=39) then
          LDShapes.BrushColour ("Polygon"+x "gray")
        else
          LDShapes.BrushShape("Polygon"+x LDShapes.BrushImage(fl[154+Math.GetRandomNumber(2)] ))
        endif
      Else
        LDShapes.BrushColour ("Polygon"+x "gray")
      endif
    endfor
  elseIf lc=8 Then
    gmm="true
  elseIf lc=3 Then
    For x=1 To 59
      op="Polygon"+x 
      If mmn[x]>0 then
        
        LDShapes.BrushShape (op LDShapes.BrushImage(fl[mmn[x]]))
      Else
        LDShapes.BrushColour (op "gray")
      endif
    endfor
    
  elseIf lc=6 Then
    For x=1 To 59
      op="Polygon"+x 
      If Array.ContainsValue(eur x) then 
        LDShapes.BrushShape(op LDShapes.BrushImage(fl[155+Math.Remainder(x 2)]))
      ElseIf mmn[x]>0 then 
        
        LDShapes.BrushShape (op LDShapes.BrushImage(fl[mmn[x]]))
      Else
        LDShapes.BrushColour (op "gray")
      endif
      
    endfor
    
  elseIf lc=2 or lc=4 Then
    For x=1 To 59
      If Array.ContainsValue(eum x) or (lc=4 and x=30)  or (lc=4 and x=2) then
        If lc=4 and ((x>=8 and x<=10) or (x=45)) then
          LDShapes.BrushColour ("Polygon"+x "blue")
        elseif lc=4 and (x>=16 and x<=18) then
          LDShapes.BrushColour ("Polygon"+x "gray")
        else
          LDShapes.BrushShape("Polygon"+x LDShapes.BrushImage(fl[36] ))
        endif
      Else
        LDShapes.BrushColour ("Polygon"+x "gray")
      endif
    endfor
  endif
EndSub '-------------------------------------------------==========================================

