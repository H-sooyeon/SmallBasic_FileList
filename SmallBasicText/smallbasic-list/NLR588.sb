Sub fld
  
  mx=wm[ww]
  pp= LDShapes.AddPolygon (mx)
  If ww=0 then 
    cl=LDColours.HSLtoRGB (210 .7 .85)
    LDShapes.AnimateOpacity (pp 7500 0)
  Else
    cl=LDColours.HSLtoRGB (250 1 Math.GetRandomNumber (10)/20+.2)
  endif
  LDShapes.BrushColour (pp cl)
  
  If ww>0 then
    LDShapes.PenWidth (pp 3)
    LDShapes.PenColour (pp "cyan")
    LDEffect.DropShaddow (pp "")
    LDShapes.SetShapeEvent (pp)
  endif
  
  ww=ww+1
  mx=0
  i=1
  
EndSub 

LDShapes.ShapeEvent=seew

While "true"'===========================================main loop
  
  If gmm Then
    gmm="false
    Shapes.HideShape (eu)
    GraphicsWindow.Title ="Game: Guess the State by flag
    For x=2 to 59
      op="Polygon"+x 
      LDShapes.AnimateOpacity  (op 0 0) 
      LDShapes.BrushColour (op "gray")
      emt[x]=1
      If Array.ContainsValue (xcc x) then
        emt[x]=0
      endif
    endfor
    gmd="true
    nxtt()
    
  Elseif cor then 'correct answer
    cor="false
    LDShapes.ZIndex (stt[1] 1500)
    stx=ldtext.Split (sttx[ess] "#")
    etx=ldtext.Split (ldtext.Replace (stx[1] "$" "@") "@")
    Shapes.SetText ( stt[1] etx[1])
     LDShapes.ZIndex (es 1500)
    LDShapes.AnimateZoom (stt[1] 500 3 2 2)
    LDShapes.AnimateZoom (es 500 3 1.2 1.2)
    LDShapes.BrushShape(es LDShapes.BrushImage(fl[fa[ess]] ))
    Program.Delay (1500)
    
    If qcc+ecc+1>40 Then
      qcc=qcc+1
      GraphicsWindow.ShowMessage ("Result: "+((qcc-ecc)/50)*100 "Game Over")
      gmd="false
      gmm="true
      qcc=0
      ecc=0
    else
      nxtt ()
      qcc=qcc+1
    endif
  endif
endwhile'====================================================endmain

Sub nxtt
  nn:
  qs=math.GetRandomNumber (58)+1
  If emt[qs]=1 then
    emt[qs]=0
    If Array.ContainsValue(eur qs) then 
      LDShapes.BrushShape(ms LDShapes.BrushImage(fl[155] ))
    Else
      LDShapes.BrushShape(ms LDShapes.BrushImage(fl[mmn[qs]] ))
    EndIf
    Shapes.SetText ( stt[1] "???")'/"+qs)
    For r=2 To 7
      Shapes.SetText ( stt[r] "")
    EndFor
    LDShapes.BrushShape(zs LDShapes.BrushImage(fl[fa[qs]] ))
  Else
    Goto nn
  endif
EndSub 


Sub kkk
  nn=text.GetCharacter (13)
  ll= text.ConvertToLowerCase (GraphicsWindow.LastKey)
  if ll="tab" Then
    ww=ww-1
    GraphicsWindow.Title =ww
  ElseIf ll="return" then
    LDShapes.AddPolygon (mx)
    
  elseif ll="q" Then
    
    ww=ww+1
    GraphicsWindow.Title =ww
  elseif ll="s" Then
    sww="true
  elseif ll="f1" Then
    lll="true
  elseif ll="left" Then
    cx=cx+30
  elseif ll="right" Then
    cx=cx-30
  elseif Text.IsSubText(ll "shift") Then
    cy=cy+30
  elseif ll="down" Then
    cy=cy-30
  EndIf 
  
  LDGraphicsWindow.Reposition (zz zz cx cy 0)
  
EndSub

Sub mww
  zz=zz+ LDEvents.LastMouseWheelDelta /30
  LDGraphicsWindow.Reposition (zz zz cx cy 0)
EndSub 

Sub seew
  es= LDShapes.LastEventShape
  
  ess=text.GetSubTextToEnd (es 8)
  If gmd Then 'game 
    if LDShapes.LastEventType="MouseDown" then 
      if ess=qs then
        cor="true
      Else
        ecc=ecc+1
      endif
      
      GraphicsWindow.Title="Correct: "+qcc+" * Wrong: "+ecc
    endif
  else
    cl=LDColours.HSLtoRGB (250 1 Math.GetRandomNumber (10)/20+.2)
    If os=0 Then
    else
      LDShapes.BrushColour (os cl)
      LDShapes.AnimateOpacity  (os 0 0) 
    endif
    
    LDShapes.BrushShape(es LDShapes.BrushImage(fl[fa[ess]] ))
    LDShapes.BrushShape(zs LDShapes.BrushImage(fl[fa[ess]] ))
    stx=ldtext.Split (sttx[ess] "#")
    etx=ldtext.Split (ldtext.Replace (stx[1] "$" "@") "@")
    If Array.GetItemCount (etx)>1 Then 
      cpc=etx[2]
      If cpc=0 Then
        cpc=cap[Text.GetSubText (stx[1]  1 4)]
      EndIf 
    Else   
      cpc=cap[Text.GetSubText (stx[1]  1 4)]
    endif
    Shapes.SetText ( stt[1] etx[1])
    Shapes.SetText ( stt[7] "∙ money: "+stx[6])
    ctt=etx[1]
    Shapes.SetText ( stt[2] stx[3]+" "+stx[2])
    Shapes.SetText ( stt[3] "∙ area km²: "+LDUtilities.GetCurrentCultureNumber (stx[4]))
    Shapes.SetText ( stt[4] "∙ Capital: "+cpc+"|inhb. "+etx[3])
    If Array.ContainsValue(eur ess) then 
      LDShapes.BrushShape(ms LDShapes.BrushImage(fl[155] ))
    Else
      LDShapes.BrushShape(ms LDShapes.BrushImage(fl[mmn[ess]] ))
    EndIf
    If Text.IsSubText (stx[2] "/EU") Then
      Shapes.ShowShape (eu)
      
    Else
      Shapes.HideShape (eu)
    endif
    LDShapes.AnimateOpacity  (es 1618 0) 
    LDShapes.ZIndex (es 1000)
    
  endif
  os=es
EndSub 
