Turtle.Speed=10

Turtle.Angle=-60

GraphicsWindow.BrushColor="Yellow"

GraphicsWindow.FillEllipse(307,241,35,35)

GraphicsWindow.PenColor="Pink"

For i = 1 To 5

  Turtle.Move(50)

  For a = 1 To 100

    Turtle.Move(0.5)

    Turtle.Turn(2)

  EndFor

  Turtle.Move(72)

  Turtle.Angle=-60+72*i

EndFor

Turtle.Hide()


