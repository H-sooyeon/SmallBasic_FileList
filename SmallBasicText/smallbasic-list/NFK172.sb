Sub stt

  Program.Delay(3333)

  GraphicsWindow.Clear()

  r = 150

  xo =hc

  yo =vc - 20

  GraphicsWindow.PenWidth=2

  GraphicsWindow.PenColor = "Gray"

  GraphicsWindow.BrushColor="transparent

  shapes.move(shapes.AddEllipse( 2 * r, 2 * r) xo - r, yo - r)

  shapes.addLine(xo - (r + 20), yo, xo + (r + 20), yo)

  shapes.addLine(xo, yo - (r + 20), xo, yo + (r + 20))

  GraphicsWindow.BrushColor = "Gray"

  shapes.move(shapes.AddEllipse(10 10) xo + 40, yo - 18 )

  _a = Math.GetRadians(30)

  x = xo + (r + 20)

  y = yo - (r + 20) * Math.Tan(_a)

  shapes.addLine(xo, yo, x, y)

  GraphicsWindow.PenColor = "Red"

  x1 = xo + r

  y1 = yo

  x2 = x1

  y2 = yo - r * Math.Tan(_a)

  shapes.addLine(x1, y1, x2, y2)

  GraphicsWindow.FontName = "Trebuchet MS"

  GraphicsWindow.FontSize = 50

  GraphicsWindow.BrushColor = "gold

  x = gw - 20 - 190

  y = gh - 20 - 50

  shapes.move(shapes.addText("Tangent") x y)

EndSub



