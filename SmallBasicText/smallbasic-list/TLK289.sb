Sub OnMDTE

  active = ""

  sbCode = sbCode + shape + " = Shapes.AddText("+dd+Text+dd+")" + lf

EndSub



Sub OnMDMLTB

  If active = "T" Then

    active = ""

    sbCode = sbCode + shape + " = Controls.AddMultiLineTextBox("+Shapes.GetLeft(shape)+","+(Shapes.GetTop(shape)-80)+")" + lf

    sbCode = sbCode + "Controls.SetSize("+shape+","+width+","+height+")" + lf

  EndIf

EndSub



Sub OnMDTB

  If active = "T" Then

    active = ""

    sbCode = sbCode + shape + " = Controls.AddTextBox("+Shapes.GetLeft(shape)+","+(Shapes.GetTop(shape)-80)+")" + lf

    sbCode = sbCode + "Controls.SetSize("+shape+","+width+","+height+")" + lf

  EndIf

EndSub



Sub OnMDB

  If active = "T" Then

    active = ""

    sbCode = sbCode + shape + " = Controls.AddButton("+dd+caption+dd+","+Shapes.GetLeft(shape)+","+(Shapes.GetTop(shape)-80)+")" + lf

    sbCode = sbCode + "Controls.SetSize("+shape+","+width+","+height+")" + lf

  EndIf

EndSub



Sub OnMMB

  If active = "T" Then

    Shapes.Move(shape,GraphicsWindow.MouseX,GraphicsWindow.MouseY-(height+10))

  EndIf

EndSub



Sub OnMM

  If active = "T" Then

    Shapes.Move(shape, GraphicsWindow.MouseX, GraphicsWindow.MouseY)

  EndIf

EndSub



Sub OnMD

  If active = "T" Then

    active = ""

    

    sbCode = sbCode + "Shapes.Move("+shape+","+GraphicsWindow.MouseX+","+(GraphicsWindow.MouseY-80)+")" + lf

  EndIf

EndSub


