     Controls.ButtonClicked = ButtonCycle

 

 Sub ButtonCycle 

   

   If Controls.LastClickedButton = Meaning[U1] Then

    GraphicsWindow.FontSize = 12

     GraphicsWindow.DrawBoundText(10,105,230,EXU1[x])

   EndIf

   

   If Controls.LastClickedButton = Economic[U1] Then 

     GraphicsWindow.FontSize = 11

     GraphicsWindow.DrawBoundText(10,105,230, U1Ecomonic[x]) 

     EndIf 

     

      If Controls.LastClickedButton = Social[U1] Then 

     GraphicsWindow.FontSize = 11

     GraphicsWindow.DrawBoundText(265,105,230, U1Social[x]) 

     EndIf 

     

      If Controls.LastClickedButton = Environment[U1] Then 

     GraphicsWindow.FontSize = 11

     GraphicsWindow.DrawBoundText(515,105,230, U1Environmental[x]) 

   EndIf 

   

   If Controls.LastClickedButton = ShowHazards Then 

     GraphicsWindow.FontSize = 15

     GraphicsWindow.DrawBoundText(25,150,230, "Corrosive") 

     GraphicsWindow.DrawBoundText(350,150,100, "Caution (Harmful or irritant)") 

     GraphicsWindow.DrawBoundText(25,340,230, "Flammable") 

     GraphicsWindow.DrawBoundText(350,340,230, "Toxic") 

   EndIf 

   

   If Controls.LastClickedButton = Reveal[U1] Then 

     GraphicsWindow.DrawResizedImage(EXU1[99],15,100,200,200)

   EndIf 

   

   If Controls.LastClickedButton = Rev[U1] Then 

    GraphicsWindow.DrawResizedImage(EXU1[99],15,100,400,400)

  EndIf 

  

     If Controls.LastClickedButton = End[1] Then

     Program.End()

   EndIf

 

  If Controls.LastClickedButton = Replay[1] Then

     GraphicsWindow.Clear()

    U1()

  EndIf

  EndSub


