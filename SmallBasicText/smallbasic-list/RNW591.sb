MAG = 200

WIDTH = MAG * 3

HEIGHT = MAG * 2

DIAMETER = HEIGHT * 3 / 5

GraphicsWindow.Title = "Japan"

GraphicsWindow.Width = WIDTH

GraphicsWindow.Height = HEIGHT

GraphicsWindow.BrushColor = "#D7003A"

x = (WIDTH - DIAMETER) / 2

y = (HEIGHT - DIAMETER) / 2 

GraphicsWindow.FillEllipse(x, y, DIAMETER, DIAMETER)


