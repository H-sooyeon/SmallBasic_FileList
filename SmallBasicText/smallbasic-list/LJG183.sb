    For L=1 To Array.GetItemCount(shape[10]) ' *********

      Shapes.Rotate(SHP["10:"+k][L],Shape[10][L]["angle"]) ' *********        ' <--------------  rotating angle // body = 8 // BlueCar index="4:1" to "4:7"  

    endfor  

  endfor

  Shapes.HideShape(oEyeLeft2)   ''MM

