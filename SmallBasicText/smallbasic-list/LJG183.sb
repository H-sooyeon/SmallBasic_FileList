  For k=1 To Array.GetItemCount(s[10])   '  k=1 to 7 

    For L=1 To Array.GetItemCount(shape[10]) ' *********

      Shapes.Rotate(SHP["10:"+K][L], 0)   ' *********      ' <--------------  rotating angle // body = 8// BlueCar index="4:1" to "4:7"  

