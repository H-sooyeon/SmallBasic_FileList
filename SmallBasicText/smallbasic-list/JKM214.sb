GraphicsWindow.MouseDown =  Down5 
Sub Down5           
  x = GraphicsWindow.MouseX 
  y = Graphicswindow.MouseY 
  aa = aa + 1
  GraphicsWindow.BrushColor = "White"
  GraphicsWindow.FillRectangle(x, y, 2, 2)
  If aa = 1 Then
    X1 = GraphicsWindow.MouseX
    ax = GraphicsWindow.MouseX
    Controls.SetTextBoxText(t11,X)
    Y1 = Graphicswindow.MouseY
    by = Graphicswindow.MouseY
    Controls.SetTextBoxText(t12,Y)
    plg[0][ X ] = GraphicsWindow.MouseX 
    plg[0][ Y ] = Graphicswindow.MouseY 
  ElseIf aa = 2 Then
    X2 = GraphicsWindow.MouseX
    cx = GraphicsWindow.MouseX
    Controls.SetTextBoxText(t11,X)
    Y2 = Graphicswindow.MouseY
    dy = Graphicswindow.MouseY
    Controls.SetTextBoxText(t12,Y)
    plg[1][ X ] = GraphicsWindow.MouseX 
    plg[1][ Y ] = Graphicswindow.MouseY  
  ElseIf aa = 3 Then
    Controls.SetTextBoxText(t11,X) 
    Controls.SetTextBoxText(t12,Y)
    plg[2][ X ] = GraphicsWindow.MouseX 
    plg[2][ Y ] = Graphicswindow.MouseY  
  ElseIf aa = 4 Then
    Controls.SetTextBoxText(t11,X) 
    Controls.SetTextBoxText(t12,Y)
    plg[3][ X ] = GraphicsWindow.MouseX 
    plg[3][ Y ] = Graphicswindow.MouseY 
  ElseIf aa = 5 Then
    Controls.SetTextBoxText(t11,X) 
    Controls.SetTextBoxText(t12,Y)
    plg[4][ X ] = GraphicsWindow.MouseX 
    plg[4][ Y ] = Graphicswindow.MouseY   
    aa = 0
  EndIf
EndSub
