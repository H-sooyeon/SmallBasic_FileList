Sub Grid
  GraphicsWindow.PenWidth = 0.5                                                                        
  GraphicsWindow.PenColor = GraphicsWindow.GetColorFromRGB(255,255,255)
  For gg = 1 To 245 Step 2
    GraphicsWindow.DrawLine(4 * gg,4,4 * gg,725)                    'gorizontal
  EndFor
  For vv = 1 To 180 Step 2
    GraphicsWindow.DrawLine(4,4 * vv,960,4 * vv)                    'vertical
  EndFor  
  For gg = 1 To 245 Step 12
    GraphicsWindow.DrawLine(4 * gg,4,4 * gg,725)                  
  EndFor
  For vv = 1 To 180 Step 12
    GraphicsWindow.DrawLine(4,4 * vv,960,4 * vv)                  
  EndFor  
EndSub

