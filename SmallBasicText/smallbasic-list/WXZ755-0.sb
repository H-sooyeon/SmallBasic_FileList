GraphicsWindow.Width = 640

GraphicsWindow.Height = 400

GraphicsWindow.Show()

For y = 1 To 399

  b = (y * -.000007) +.1014

  For x = 1 To 639

    a = (x * .0000062) - .75

    v = 0

    w = 0

    i = 0

    iter:

    c = (v * v) - (w * w) + a

    d = (2 * w * v) + b

    v = c

    w = d 

    i = i + 1

    If w > -2 And w < 2 And v > -2 And v < 2 And i < 255 Then

      goto iter

    EndIf

    If i = 255 Then

      i=0

    EndIf

    GraphicsWindow.SetPixel(x,y,GraphicsWindow.GetColorFromRGB(i,2*i,3*i))

  EndFor

EndFor


