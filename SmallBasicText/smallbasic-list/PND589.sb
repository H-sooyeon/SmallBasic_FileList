y=0.01
K=1

GraphicsWindow.Height=700
GraphicsWindow.Width=700

 rainbow()
Turtle.PenDown()

    GraphicsWindow.backgroundcolor="BLACK"
GraphicsWindow.PenWidth=20
Turtle.Speed=10

While y<> 10000
 
 FOR x = 1 To 360 
 
    GraphicsWindow.PenColor=colours[K]
    Turtle.Move(Math.NaturalLog(y))
    Turtle.Angle=x
    y=y+0.02
    K=K+1
    If K=23 Then
      K=1
      EndIf
   
 ENDFOR
EndWhile
  
