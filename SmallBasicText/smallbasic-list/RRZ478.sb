V1 = "Anton"
V2 = "Berta"
V1 = Text.ConvertToUpperCase(V1)
V2 = Text.ConvertToUpperCase(V2) 
TextWindow.WriteLine(V1)
TextWindow.WriteLine(V2)
If (V1 < V2) Then  
  TextWindow.Write(V1)
  TextWindow.Write(" < ")
  TextWindow.Write(V2) 
EndIf 
If (V1 > V2) Then 
  TextWindow.Write(V1)
  TextWindow.Write(" > ")
  TextWindow.Write(V2) 
EndIf 
If (V1 = V2) Then
  TextWindow.Write(V1)
  TextWindow.Write(" = ")
  TextWindow.Write(V2) 
EndIf 
