For i = 1 To 999

  getDigits()

  setArray()

EndFor



printOut()



Sub getDigits

  j = Math.Floor( i / 100 )

  k = Math.Floor( (i - (j * 100)) / 10)

  l = i - ((j * 100) + (k * 10))

EndSub



Sub setArray

  If j + k + l = 10 Then

    howMany = howMany + 1

    numArray[ howMany ] = i

  EndIf

EndSub



Sub printOut

  For i = 1 To howMany

    TextWindow.WriteLine( numArray[ i ] )

  EndFor

  

  TextWindow.WriteLine( "There are " + howMany + " numbers." )

EndSub

  


