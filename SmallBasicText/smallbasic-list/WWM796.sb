Sub Direction
  Sub1:
  Direction =TextWindow.Read()
  FromText:
  If Direction="North" Or Direction="N" Or Direction="north" or Direction="n" Then
    Direction="North"
  ElseIf Direction="South" Or Direction="S" Or Direction="south" or Direction="s" Then
    Direction="South"
  Elseif Direction="East" Or Direction="E" Or Direction="east" or Direction="e" Then
    Direction="East"
  Elseif Direction="West" Or Direction="W" Or Direction="west" or Direction="w" Then
    Direction="West"
  EndIf  
  If Direction="North" And y=2 Or x=0 And y=1 And Direction="North" Or Direction="North" And x=0 And y=0 And q<>1 Then
    TextWindow.WriteLine("You cannot go this way")
    Direction()
  ElseIf Direction="South" and y=-2 or x=0 and y=2 and Direction="South" then
    TextWindow.WriteLine("You cannot go this way")
    Direction()
  ElseIf Direction="East" and x=2 or x=-1 and y=0 and Direction="East" or Direction="East" and y=1 and x=-1 or Direction="East" and x=0 and y=0 or Direction="East" and x=0 and y=1 then
    TextWindow.WriteLine("You cannot go this way")
    Direction()
  ElseIf Direction="West" and x=-2 or x=1 and y=0 and Direction="West" or Direction="West" and y=1 and x=1 or Direction="West" and x=0 and y=0 or Direction="West" and x=0 and y=1 then
    TextWindow.WriteLine("You cannot go this way")
    Direction()  
  ElseIf Direction= "North"  Then
    y= y+1
  ElseIf Direction= "South" then
    y= y-1
  Elseif Direction= "East" then
    x= x+1
  Elseif Direction= "West" then
    x= x-1
  Else 
    TextWindow.WriteLine("please retype that")
    Goto Sub1
  EndIf
  If q<> 1 Then
    Coor()
  endif 
EndSub  
Sub Coor
  Coord:  
  If x=0 And y=0 Then
    X0Y0()
  elseIf x=1 And y=0 Then
    X1Y0()
  elseIf X=1 And y=1 Then
    X1Y1()
  elseIf X=1 And y=2 Then
    X1Y2()
  elseIf X=1 And y=-1 Then
    X1Y3()
  elseIf X=1 And y=-2 Then
    X1Y4()
  elseIf X=2 And y=2 Then
    X2Y2()
  elseIf X=2 And y=1 Then
    X2Y1()
  elseIf X=2 And y=0 Then
    X2Y0()
  elseIf X=2 And y=-1 Then
    X2Y3()
  elseIf X=2 And y=-2 Then
    X2Y4()
  elseIf X=0 And y=2 Then
    X0Y2()
  elseIf X=0 And y=1 Then
    Goto X0Y1
  elseIf X=0 And y=-1 Then
    X0Y3()
  elseIf X=0 And y=-2 Then
    X0Y4()
  elseIf X=-1 And y=2 Then
    X3Y2()
  elseIf X=-1 And y=1 Then
    X3Y1()
  elseIf X=-1 And y=0 Then
    X3Y0()
  elseIf X=-1 And y=-1 Then
    X3Y3()
  elseIf X=-1 And y=-2 Then
    X3Y4()
  elseIf X=-2 And y=2 Then
    X4Y2()
  elseIf X=-2 And y=1 Then
    X4Y1()
  elseIf X=-2 And y=0 Then
    X4Y0()
  elseIf X=-2 And y=-1 Then
    X4Y3()
  elseIf X=-2 And y=-2 Then
    X4Y4()
  Else 
    Textwindow.WriteLine("CRAP!")
    Goto Start
EndIf 'Free world time, the character can explore and level up on his/her own!
