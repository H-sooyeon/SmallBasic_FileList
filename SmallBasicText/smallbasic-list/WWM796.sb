Sub Battle
  OHP=HP
  While HP >0 Or FoeHP >0 'this is my while loop it really helps in battles!
    TextWindow.WriteLine("You attack!")
    Program.Delay(2567)
    UDamage=Math.GetRandomNumber(Attack)
    FDefence=Math.GetRandomNumber(FoeD)
    If UDamage<FDefence Then
      UDamage= Math.Ceiling(UDamage/2)
    EndIf
    TextWindow.WriteLine("You did "+UDamage+" damage against the "+Foe+"")
    FoeHP = FoeHP - UDamage
    If FoeHP<1 Then
      Goto Ending
    EndIf  
    TextWindow.WriteLine("")
    TextWindow.WriteLine("It Attacks!")
    Program.Delay(2567)
    UDefence=Math.GetRandomNumber(Defence)
    FDamage= Math.GetRandomNumber(FoeA) 
    If FDamage<UDefence Then
      FDamage= Math.Ceiling(FDamage/2)
    EndIf 
    TextWindow.WriteLine(""+Foe+" did "+FDamage+" damage against the You!")
    TextWindow.WriteLine("")
    HP=HP- FDamage
    If HP<1 Then
      Goto Ending
      EndIf
    Program.Delay(1566)
    EndWhile
  Ending:    
  If HP< 0 Then
    TextWindow.WriteLine("You have Died against "+Foe+"")
    TextWindow.WriteLine("YOU LOSE")
    TextWindow.Pause()
    If Quest="Wizard" Then
      Goto Start
    EndIf  
    Program.End()
  ElseIf FoeHP<=0 then
    HP=OHP
    TextWindow.WriteLine(""+Name+" Has defeated "+Foe+"")
    TextWindow.WriteLine("And Gained "+FoeEX+"XP!")
    TextWindow.WriteLine("")
    TextWindow.PauseWithoutMessage()
    Lvl()
  EndIf  
