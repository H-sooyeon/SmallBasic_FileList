Sub TextBox 'The main descision making model, uses it every single room
  Response=TextWindow.Read()
  If Response="Direction" Then
    Direction()
  elseIf Response= "North" or Response="north" or Response="N" or Response="n" Then
     Direction= "North" 
    FromTextN()
  ElseIf Response= "East" or Response="east" or Response="E" or Response="e" then
     Direction= "East" 
    FromTextE()
  Elseif Response= "South" or Response="south" or Response="S" or Response="s" then
     Direction= "South"
     FromTextS()
  Elseif Response= "West" or Response="west" or Response="W" or Response="w" Then
    Direction= "West"
    FromTextW()
  ElseIf Response= "Attack" or Response="attack" then 
    Foes()
  Elseif Response= "Talk" or Response="talk" then
    Talk()
  Elseif Response= "Stats" or Response="stats" then 
    Stats()
  Else 
    TextWindow.WriteLine("I do not understand that command")
    TextBox()
  EndIf
EndSub
