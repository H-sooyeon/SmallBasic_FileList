Sub Lvl
  XP= FoeEX + XP 
  If XP>=XPNeed Then
    Lvl= lvl+1
    Attack=Attack+1
    Defence=Defence+1
    HP=HP+5
    XPNeed = (Lvl*XPNeed)+5
    If XPNeed>1000 Then
       XPNeed=Math.Ceiling(XPNeed/12)
    elseIf XPNeed>600 Then
      XPNeed=Math.Ceiling(XPNeed/7)'It gets much more difficult to level p as the game goes on, this helps alot.
    elseIf XPNeed>200 Then
      XPNeed=Math.Ceiling(XPNeed/3)
    EndIf
    XP=0
    TextWindow.WriteLine(""+Name+" Leveled up to Level "+Lvl+"")
    TextWindow.PauseWithoutMessage()
  EndIf
EndSub  
