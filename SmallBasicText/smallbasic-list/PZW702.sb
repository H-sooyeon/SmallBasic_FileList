  Sub CheckUp

      If Key = "Up" Then

    If Move["Up"] <> "No" Then

  Rotate = 0

  RotateTank()

    Y = Y - TankSpeed

  If Text.GetSubText(Level[Math.Round((Y - 16) / 30) + 1],Math.Round((X + 20) / 30),1) = "P" Then

Move["Up"] = "No"

EndIf

    Move["Left"] = "Y"

    Move["Right"] = "Y"

    Move["Down"] = "Y"

EndIf

EndIf

EndSub



