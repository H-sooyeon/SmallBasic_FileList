       If Playernum = 2 Then

       x10 = H

       y10 = f

       Else

       x = H

       Y = F

     EndIf



     EndIf

     

       If Block = "V" Then

         Shapes.Remove(TNK[2])

          GraphicsWindow.DrawImage(img6,H,F)

       TNK[2] = Shapes.AddImage(img4)

       Shapes.Move(TNK[2],H + 8,F + 8)

       Rotate = 0



            If Playernum = 2 Then

       x = H

       y = f

     Else

       x10 = H

       Y10 = F

       EndIf

     EndIf

     

     

  If mn = 30 AND Z < 19 Then

    Z = Z + 1

    F = F + 30

    H = 0

    Goto restart

  EndIf 

       H = H + 30

ENDFOR

EndFor

