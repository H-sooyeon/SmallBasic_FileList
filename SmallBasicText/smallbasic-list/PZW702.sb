Sub CheckRight

  If Key = "Right" Then

    If Move["Right"] <> "No" Then

  Rotate = 90

  RotateTank()

    X = X + TankSpeed

  If Text.GetSubText(Level[Math.Round((Y - 5)  / 30) + 1],Math.Round((X + 30) / 30),1) = "P" Then

Move["Right"] = "No"

EndIf

    Move["Left"] = "Y"

