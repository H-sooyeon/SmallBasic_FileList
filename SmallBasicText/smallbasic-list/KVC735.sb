    TextWindow.ForegroundColor=a5

    TextWindow.Write(Text.GetSubText(args[1] 8 2))

    

    TextWindow.CursorLeft=TextWindow.CursorLeft-3

    TextWindow.CursorTop=TextWindow.CursorTop+1

    TextWindow.Write(Text.GetSubTextToEnd(args[1] 10))

  Else

    TextWindow.Write(tw[1])

    TextWindow.BackgroundColor=dblack

    TextWindow.ForegroundColor=a5

    TextWindow.Write("-")

    TextWindow.CursorLeft=TextWindow.CursorLeft-Text.GetLength(tw[2])-1

    TextWindow.CursorTop=TextWindow.CursorTop+1

    TextWindow.Write(tw[2])

  EndIf

  TextWindow.ForegroundColor="black

  TextWindow.BackgroundColor=a5

EndSub


