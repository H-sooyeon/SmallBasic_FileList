Sub REScolor
  
  If newver=1 Then
    Goto hop
    EndIf
  
  If verif=1 Then
    TextWindow.ForegroundColor="darkyellow"
    Goto pre 
  EndIf
  
  hop:
  
  If num=nr1 Then 
   TextWindow.ForegroundColor="magenta"
  Else
   TextWindow.ForegroundColor="darkyellow"
  
 'EndIf     !!! supprimé !!! = sinon nr1 pas colorié
     If num=nr2 Or num=nr3 Or num=nr4 Or num=nr5 Or num=nr6 Or num=nr7 Or num=nr8 Or num=nr9 Or num=nr10 Or num=nr11 Or num=nr12 Or num=nr13 Or num=nr14 Or num=nr15 Or num=nr16 Or num=nr17 Or num=nr18 Or num=nr19 Or num=nr20 Then 
      TextWindow.ForegroundColor="red"
 Else
   TextWindow.ForegroundColor="darkyellow"
    EndIf
EndIf
pre:
EndSub 
'=====================================
Sub color
  
 If n[num]=0 Then
    TextWindow.ForegroundColor="darkyellow"
  
ElseIf n[num]=1 then
    TextWindow.ForegroundColor="darkblue "
    ElseIf n[num]=2 then
    TextWindow.ForegroundColor="blue "
    
    ElseIf n[num]=3 then
    TextWindow.ForegroundColor="cyan "
    
    ElseIf n[num]=4 then
    TextWindow.ForegroundColor="white "
    
    ElseIf n[num]>4 then
    TextWindow.ForegroundColor="green"
  EndIf
  EndSub
