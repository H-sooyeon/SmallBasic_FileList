Sub chshw

  cc=args[1]

  For f=2 To 8

    nn="Rectangle"+(f-1+dx)

    LDShapes.BrushColour(nn cll[text.GetSubText( chb[cc] 17-f 1)])

  EndFor

  

  For f=1 To 4

    nn="Rectangle"+(13-f+dx)

    LDShapes.BrushColour(nn cll[text.GetSubText( chb[cc] f 1)])

  EndFor

  LDShapes.BrushColour("Rectangle"+(8+dx) cll[text.GetSubText( chb[cc] 6 1)])

  LDShapes.BrushColour("Rectangle"+(13+dx) cll[text.GetSubText( chb[cc] 16 1)])

EndSub


