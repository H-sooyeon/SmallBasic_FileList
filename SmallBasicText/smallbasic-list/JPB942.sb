Sub init
  r=20
  d=r*2
  GraphicsWindow.Title="Molecular Challenge-1,4-dioxin(C4H4O2)"
  GraphicsWindow.Width=790
  GraphicsWindow.BackgroundColor="#dabc72"
  GraphicsWindow.PenWidth=15
  Turtle.Speed=10
  color1="Blackâ€œ
  color2="Black"
EndSub
Sub writename
  GraphicsWindow.FontBold="false"
  GraphicsWindow.FontName="Times New Roman"
  GraphicsWindow.BrushColor="White"
  GraphicsWindow.FontSize=48
  GraphicsWindow.DrawText(600,20,"C")
  GraphicsWindow.FontSize=24
  GraphicsWindow.DrawText(640,50,"4")
  GraphicsWindow.FontSize=48
  GraphicsWindow.DrawText(660,20,"H")
  GraphicsWindow.FontSize=24
  GraphicsWindow.DrawText(700,50,"4")
  GraphicsWindow.FontSize=48
  GraphicsWindow.DrawText(720,20,"O")
  GraphicsWindow.FontSize=24
  GraphicsWindow.DrawText(760,50,"2")
EndSub
Sub drawatom
  GraphicsWindow.BrushColor=GraphicsWindow.PenColor
  GraphicsWindow.FillEllipse(Turtle.X-r,Turtle.Y-r,d,d)
EndSub
Sub drawbond
  GraphicsWindow.PenColor=color1
  Turtle.Move(r+20)
  GraphicsWindow.PenColor=color2
  Turtle.Move(20+r)
EndSub
Sub draw_double_carbon_bond
  GraphicsWindow.PenColor="Black"
  GraphicsWindow.PenWidth=7.5
  Turtle.X=Turtle.X+10
  For i = 1 To 3
    Turtle.Move(r+40+r)
    Turtle.TurnLeft()
    Turtle.Move(20)
    Turtle.TurnLeft()
  EndFor
  Turtle.TurnLeft()
  Turtle.Move(10)
  Turtle.TurnLeft()
EndSub
Sub draw_double_carbon_bond2
  GraphicsWindow.PenColor="Black"
  GraphicsWindow.PenWidth=7.5
  Turtle.X=Turtle.X-10
  For i = 1 To 3
    Turtle.Move(r+40+r)
    Turtle.TurnLeft()
    Turtle.Move(20)
    Turtle.TurnLeft()
  EndFor
  Turtle.TurnLeft()
  Turtle.Move(10)
  Turtle.TurnLeft()
EndSub
