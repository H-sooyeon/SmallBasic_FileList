Sub Level4
  a = 3
  P1 = Math.GetRandomNumber(1280)
  P2 = Math.GetRandomNumber(780)
  GraphicsWindow.BrushColor = "Black"
  Controls.HideControl(Level3B)
  GraphicsWindow.FontSize = 4
