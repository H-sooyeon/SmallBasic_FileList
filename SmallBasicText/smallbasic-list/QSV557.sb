Sub pushpopwrite

  TextWindow.Write("To write in stack: ")

    push = TextWindow.Read()

  Stack.PushValue("stackable", push) 

    pop = Stack.PopValue("stackable")

  TextWindow.WriteLine("Popped from stack: " + pop)

  Stack.PushValue("stackable", push)

