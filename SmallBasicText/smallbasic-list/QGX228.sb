fistTwoPrimes = "1=2;2=3"

i = 2

n = 3



start = Clock.ElapsedMilliseconds



While (Clock.ElapsedMilliseconds - start) <= 60000 'loops upto 60s

  n = n + 2  

  isPrime = "True"

  For j = 2 To Math.SquareRoot(n)

    If (Math.Remainder(n, j) = 0) Then

      isPrime = "False"

    EndIf

  EndFor

  If isPrime Then

    i = i + 1

    prime = n

  EndIf

EndWhile



TextWindow.Write(i + ": ")

TextWindow.Write(prime + "  ")


