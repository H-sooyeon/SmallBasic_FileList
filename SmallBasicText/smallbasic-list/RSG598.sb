Sub sleeps
  If sleepsettings = 0 Then
    Time = Time + 10
    sleep = sleep + 1
    If AChealth - health <= 19 Then
      health = AChealth
    Else
      health = health + 20
    EndIf
    ticker()
    statsbar()
    houseoptions = 10
    home()
  EndIf
EndSub
