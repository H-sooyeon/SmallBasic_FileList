Sub MoveCycle
  key= GraphicsWindow.LastKey
  delX = 0
  delY = 0
  If (key = "Up") Then
    delY = -2
  ElseIf (key = "W") then
    delY = -2
  ElseIf (key = "Down") Then
      delY = 2
  elseif (key = "S") then
      delY = 2
  ElseIf (key = "Left") Then
    delX = -2
  elseif (key = "A") then
    delX = -2
  ElseIf (key = "Right") Then
    delX = 2
  elseif (key = "D") then
    delX = 2
  EndIf
  X = Shapes.GetLeft(Character)
  Y = Shapes.GetTop(Character)
  If characterdot = 0 Then
  Shapes.Move(Character, X + delX, Y + delY)
EndIf
EndSub
