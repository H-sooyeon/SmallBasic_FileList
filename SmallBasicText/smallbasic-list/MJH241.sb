Sub csr
  If args[2]=args[3] Then
    
    Return=args[1]+"/"+args[2]
    
  ElseIf args[2]="" Then
    Return=args[1]+"/"+LDText.Replace(LDText.Replace(LDText.Replace(LDText.Replace(args[1] "ium" "ij") "ll" "l") "y" "i") "x" "ks")
    
  Else
    
    Return=args[1]+"/"+args[2]+"/"+args[3]
  EndIf
  
EndSub
