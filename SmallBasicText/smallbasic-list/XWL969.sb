Sub maindrw
  
  scal=ldtext.split("1.2 1.2 1 1 1 1 1 1.05 1.05 1.05 1 1" " ")
  
  for qi=3 to 12'------------gemm--sagg
    scale=scal[qi]
    edge=edg[qi]
    shX=ashX[qi]
    shY=ashy[qi]  
    wrap=wra[qi]
    If qi=5 Then 
      Str_tau()
    Else
      
      star=ldtext.Split (sti[qi] "|")
    endif
    shape=ldtext.Split (ssi[qi] "|")
    an = 0
    
    If qi=3 Then
      zii=200
      num = Array.GetItemCount(star)
      index = Array.GetAllIndices(star)
      offsetX = 0
      offsetY = 0
      GetMinMax()
      offsetX = -xMin + 50
      offsetY = -yMin + 90
    Else
      zii=1  
    endif
    Shapes_Add()
    sdrw()  
  endfor  
  
  
  for qi=1 to 2'------------cc-aq
    scale=scal[qi]
    edge=edg[qi]
    shX=ashX[qi]
    shY=ashy[qi]  
    wrap=wra[qi]
    
    If qi=2 Then
      Str_aq ()
    else
      star=ldtext.Split (sti[qi] "|")
    endif
    shape=ldtext.Split (ssi[qi] "|")
    an = 0
    zii=1  
    Shapes_Add()
    sdrw()  
  endfor  
  
  LDFastShapes.Update ()
  ds=95
  
  For s= 1 to 12  
    dooff ()
    Program.Delay (333)
  endfor
  LDEvents.MouseWheel=wll
  inn=0
EndSub 


Sub wll
  If inn=0 then
    inn=1
    ds=LDEvents.LastMouseWheelDelta *65
    off=off+ds
    'GraphicsWindow.Title=off
    dooff()
    inn=0
  endif
EndSub 


