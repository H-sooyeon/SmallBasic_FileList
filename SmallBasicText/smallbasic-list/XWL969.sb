Sub Shapes_Add
  j=1
  Stack.PushValue("local", i)
  Stack.PushValue("local", x)
  Stack.PushValue("local", y)
  Shapes_CalcWidthAndHeight()
  s = scale
  
  For i = 1 To Array.GetItemCount(shape)
    shp = shape[i]
    GraphicsWindow.PenWidth = shp["pw"] * s
    If shp["pw"] > 0 Then
      GraphicsWindow.PenColor = shp["pc"]
    EndIf
    If Text.IsSubText("rect|ell|tri|text", shp["fn"]) Then
      GraphicsWindow.BrushColor = shp["bc"]
    EndIf
    If shp["fn"] = "rect" Then
      shpp = Shapes.AddRectangle(shp["wd"] * s, shp["ht"] * s)
    ElseIf shp["fn"] = "ell" Then
      shpp = Shapes.AddEllipse(shp["wd"] * s, shp["ht"] * s)
    ElseIf shp["fn"] = "tri" Then
      shpp = Shapes.AddTriangle(shp["x1"] * s, shp["y1"] * s, shp["x2"] * s, shp["y2"] * s, shp["x3"] * s, shp["y3"] * s)
    ElseIf shp["fn"] = "line" Then
      shpp = Shapes.AddLine(shp["x1"] * s, shp["y1"] * s, shp["x2"] * s, shp["y2"] * s)
      
    EndIf
    LDShapes.ZIndex (shpp,zii)
    zii=zii+1
    x = shp["x"]
    y = shp["y"]
    shp["rx"] = x
    shp["ry"] = y
    ffs[qi][j]=LDFastShapes.ShapeIndex(shpp)   
    ldfastShapes.Move(ffs[qi][j], shX + x * s, shY + y * s)
    If Text.IsSubText("rect|ell|tri", shp["fn"]) And (shp["an"] <> 0) And (shp["an"] <> "") Then
      ldfastShapes.Rotate(ffs[qi][j], shp["an"])
    EndIf
    'shape[i] = shpp
    
    j=j+1
  EndFor
  LDFastShapes.Update ()
  shAngle = 0
  y = Stack.PopValue("local")
  x = Stack.PopValue("local")
  i = Stack.PopValue("local")
  ttt:
EndSub

Sub Shapes_CalcWidthAndHeight
  For i = 1 To Array.GetItemCount(shape)
    shp = shape[i]
    If shp["fn"] = "tri" Or shp["fn"] = "line" Then
      xmin = shp["x1"]
      xmax = shp["x1"]
      ymin = shp["y1"]
      ymax = shp["y1"]
      If shp["x2"] < xmin Then
        xmin = shp["x2"]
      EndIf
      If xmax < shp["x2"] Then
        xmax = shp["x2"]
      EndIf
      If shp["y2"] < ymin Then
        ymin = shp["y2"]
      EndIf
      If ymax < shp["y2"] Then
        ymax = shp["y2"]
      EndIf
      If shp["fn"] = "tri" Then
        If shp["x3"] < xmin Then
          xmin = shp["x3"]
        EndIf
        If xmax < shp["x3"] Then
          xmax = shp["x3"]
        EndIf
        If shp["y3"] < ymin Then
          ymin = shp["y3"]
        EndIf
        If ymax < shp["y3"] Then
          ymax = shp["y3"]
        EndIf
      EndIf
      shp["wd"] = xmax - xmin
      shp["ht"] = ymax - ymin
    EndIf
    If i = 1 Then
      shWidth = shp["x"] + shp["wd"]
      shHeight = shp["y"] + shp["ht"]
    Else
      If shWidth < shp["x"] + shp["wd"] Then
        shWidth = shp["x"] + shp["wd"]
      EndIf
      If shHeight < shp["y"] + shp["ht"] Then
        shHeight = shp["y"] + shp["ht"]
      EndIf
    EndIf
    shape[i] = shp
  EndFor
EndSub

Sub Shapes_Move
  
  Stack.PushValue("local", i)
  s = scale
  shX = x
  shY = y
  For i = 1 To Array.GetItemCount(shape)
    shp = shape[i]
    
    _x = shp["rx"]
    _y = shp["ry"]
    
    Shapes.Move(shp["obj"], shX + _x * s, shY + _y * s)
  EndFor
  i = Stack.PopValue("local")
EndSub

