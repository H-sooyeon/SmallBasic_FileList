if (a1>b1) Then
  GraphicsWindow.DrawText(320, 320,"n="+n )
   GraphicsWindow.DrawText(320, 380, "u="+u)
      Else
     GraphicsWindow.DrawText(600, 320,"n="+n )
   GraphicsWindow.DrawText(600, 380, "u="+u)
EndIf
GraphicsWindow.DrawText(440, 460,n)
GraphicsWindow.DrawText(470, 460,u)   
   GraphicsWindow.DrawText(440, 530, "Төгсгөл")
 EndSub



