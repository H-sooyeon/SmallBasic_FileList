Sub elp
sides = 5
length = 30 / sides
angle = -90 / sides
Turtle.Turn(90)
Turtle.Move(50)
   For i = 1 To sides*2
     Turtle.Move(length)
     Turtle.Turn(angle)
   EndFor
   sides=sides*2
   Turtle.Move(100)
    For i = 1 To sides
     Turtle.Move(length)
     Turtle.Turn(angle)
   EndFor
   Turtle.Move(50)
   EndSub

