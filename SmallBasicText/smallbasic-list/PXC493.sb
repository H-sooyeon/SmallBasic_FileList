Sub mww

  If Mouse.IsLeftButtonDown Then

    aa=aa+LDEvents.LastMouseWheelDelta*5

  else

    zz=zz+ LDEvents.LastMouseWheelDelta /20

  endif

  LDGraphicsWindow.Reposition (zz zz 0 0 aa)

EndSub 



