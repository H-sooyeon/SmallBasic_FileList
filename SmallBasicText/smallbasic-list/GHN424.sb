'Default'

Sub Default

  If Turtle.X > 900 Then

    Turtle.PenUp()

    Turtle.MoveTo(40*n + 25, m)

    Turtle.Angle = 0

    Turtle.Move(20)

    If doingreset = 0 then

      Turtle.PenDown()

    ElseIf doingreset = 1 then

      doingreset = 0

    EndIf

    Turtle.Turn(90)

    Turtle.Move(20)

    m = m + 50

    n = 0

  EndIf

  Turtle.PenUp()

  Turtle.MoveTo(40*n + 25, m)

  Turtle.Angle = 0

  Turtle.PenDown()

EndSub

'End Default'



