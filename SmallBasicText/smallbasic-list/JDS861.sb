Sub mww

  If Mouse.IsLeftButtonDown Then

    aa=aa+LDEvents.LastMouseWheelDelta*3

  Else

    rr=rr+LDEvents.LastMouseWheelDelta*12

  EndIf

  ldGraphicsWindow.Reposition(1 1 0 rr aa)

EndSub



