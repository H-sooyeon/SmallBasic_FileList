Sub Drawcharacter

  If color = "Red" Then

  charactercolor = red

  Elseif color = "Blue" Then

    charactercolor = blue

  elseif color = "Orange" Then

    charactercolor = orange

  elseif color = "Yellow" then

    charactercolor = yellow

  elseif color = "Green" then

    charactercolor = lime

  elseif color = "Purple" then

    charactercolor = purple

  Else

    charactercolor = blue

EndIf

