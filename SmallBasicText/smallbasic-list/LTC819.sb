'==========

Sub realtime

  seconds = seconds + 1

  If seconds >= 60 Then

    seconds = seconds - 60

    minutes = minutes + 1

  EndIf

  if minutes >= 60 Then

    minutes = minutes - 60

    hours = hours + 1

  EndIf 

EndSub

