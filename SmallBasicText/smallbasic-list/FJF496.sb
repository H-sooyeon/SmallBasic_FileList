          _x1 = shape[i]["x1"]

          _y1 = shape[i]["y1"]

          _x2 = shape[i]["x2"]

          _y2 = shape[i]["y2"]

          _x3 = shape[i]["x3"]

          _y3 = shape[i]["y3"]

          oFrame[func] = Shapes.AddTriangle(_x1, _y1, _x2, _y2, _x3, _y3)

