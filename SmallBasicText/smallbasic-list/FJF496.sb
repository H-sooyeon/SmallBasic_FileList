Sub KB_FlushFIFO

  For out = out + 1 To in

    fifok[out] = ""

    fifos[out] = ""

    fifoc[out] = ""

  EndFor    

EndSub



Sub KB_InKey

  c = ""

  If in > out Then

    out = out + 1

    c = fifok[out]

    _shift = fifos[out]

    _ctrl = fifoc[out]

    fifok[out] = ""

    fifos[out] = ""

    fifoc[out] = ""

    If Text.GetLength(c) > 1 Then

      If Array.ContainsIndex(keys, c) Then

        c = keys[_shift + c]

      Else

        c = "<" + c + ">" 

      EndIf

    ElseIf _shift = "" Then

      c = Text.ConvertToLowerCase(c)

    EndIf

    c = Text.Append(_ctrl, c)

  EndIf

EndSub  



