    GraphicsWindow.DrawLine(yellow2start,y,yellow2ends,y)

    GraphicsWindow.PenColor = "darkgreen "

    GraphicsWindow.DrawLine(yellowends,y,yellow2start,y)

    yellowends = yellowends + 1.4

    yellow2start = yellow2start -1.4 

  EndFor 

  

  For y = 40 To 220.5

    Program.Delay(1)

    GraphicsWindow.PenColor = "black"

    GraphicsWindow.DrawLine(0,y,yellowstart,y)

    GraphicsWindow.DrawLine(624,y,yellow2ends,y)

    GraphicsWindow.PenColor = "yellow"

    GraphicsWindow.DrawLine(yellowstart,y,yellowends,y)

    GraphicsWindow.DrawLine(yellow2start,y,yellow2ends,y)

    GraphicsWindow.PenColor = "darkgreen"

    If yellow2start - yellowends < 0 Then 

      Goto skip 

    Else 

      GraphicsWindow.DrawLine(yellowends,y,yellow2start,y)

    EndIf 

    skip:

    yellowends = yellowends + 1.4

    yellow2start = yellow2start -1.4

    yellowstart = yellowstart + 1.4

    yellow2ends = yellow2ends - 1.4

  EndFor 

  

  For y = 220 To 401

    Program.Delay(1)

    GraphicsWindow.PenColor = "black"

    GraphicsWindow.DrawLine(0,y,yellowstart,y)

    GraphicsWindow.DrawLine(624,y,yellow2ends,y)

    GraphicsWindow.PenColor = "yellow"

    GraphicsWindow.DrawLine(yellowstart,y,yellowends,y)

    GraphicsWindow.DrawLine(yellow2start,y,yellow2ends,y)

    GraphicsWindow.PenColor = "darkgreen"

    If yellow2start - yellowends < 0 Then 

      Goto skip2 

    Else 

      GraphicsWindow.DrawLine(yellowends,y,yellow2start,y)

    EndIf 

    skip2:

    yellowends = yellowends - 1.4

    yellow2start = yellow2start + 1.4

    yellowstart = yellowstart - 1.4

    yellow2ends = yellow2ends +  1.4

  EndFor 

  

  yellowstart = 0

  yellowends = yellowstart + 50

  yellow2start = 624 - 50

  yellow2ends = 624 

  For  y= 441 To 401 Step -1

    Program.Delay(1)

    GraphicsWindow.PenColor = "yellow"

    GraphicsWindow.DrawLine(yellowstart,y,yellowends,y)

    GraphicsWindow.DrawLine(yellow2start,y,yellow2ends,y)

    GraphicsWindow.PenColor = "darkgreen "

    GraphicsWindow.DrawLine(yellowends,y,yellow2start,y)

    yellowends = yellowends + 1.4

    yellow2start = yellow2start -  1.4 

  EndFor

  

  '----------Include with each flag ---------------------------------------------------------------------

  CountryName = "Jamaica"

  NameAndPause()

  '---------------------------------------------------------------------------------------------------------

EndSub 



Sub Japan

  GraphicsWindow.BackgroundColor = "black"

  r = 73.5 

  pi = Math.Pi

  For y = 0 To 147

    Program.Delay(3)

    GraphicsWindow.PenColor = "white"

    GraphicsWindow.DrawLine(0,y,624,y)

  EndFor 

  

  For count = 0 To pi*2 Step 0.0001                      

    xc = r * Math.Cos(count)

    yc = r * Math.Sin(count)

    If count < pi/2 or count > (3*pi)/2 Then  

      drawwhitesR()

      GraphicsWindow.PenColor = "red"

      GraphicsWindow.DrawLine(312,220,312 + xc,220 + yc) 

    ElseIf count > pi/2 and count < (3*pi)/2 then 

      drawwhitesL() 

      GraphicsWindow.PenColor = "red" 

      GraphicsWindow.DrawLine(312,220,312 + xc,220 + yc) 

    EndIf 

  EndFor

  

  For y = 294 to 441

    Program.Delay(3) 

    GraphicsWindow.PenColor = "white"

    GraphicsWindow.DrawLine(0,y,624,y)

  endfor

  '----------Include with each flag ---------------------------------------------------------------------

  CountryName = "Japan"

  NameAndPause()

  '----------------------------------------------------------------------------------------------------------



EndSub



 









































































































































Sub drawwhitesL

  GraphicsWindow.PenColor = "white"

  GraphicsWindow.DrawLine(0,220 + yc,312 + xc,220 + yc)

EndSub 



Sub drawwhitesR

  GraphicsWindow.PenColor = "white"

  GraphicsWindow.DrawLine(624,220 + yc,312 + xc,220 + yc)

EndSub



