Sub Usa 

  x1 = 0 

  x2 = 250 'To blue box

  x3 = 624 'To right edge

  '===================================================================

  ' Top part of the flag:  Blue box with red/white stripes:

  

  For y = 1 to 220.5

    Program.Delay(1)

    GraphicsWindow.PenColor = "blue"

    GraphicsWindow.DrawLine(x1,y,x2,y)

    '================================

    If y < 31.5 Then 

      GraphicsWindow.PenColor = "red"

      GraphicsWindow.DrawLine(x2,y,x3,y)

    ElseIf y > 31.5 and y < 63 then 

      GraphicsWindow.PenColor = "white"

      GraphicsWindow.DrawLine(x2,y,x3,y)

    ElseIf y > 63 and y < 94.5 then 

      GraphicsWindow.PenColor = "Red"

      GraphicsWindow.DrawLine(x2,y,x3,y)

    ElseIf y > 94.5 and y < 126 then 

      GraphicsWindow.PenColor = "white"

      GraphicsWindow.DrawLine(x2,y,x3,y)

    ElseIf y > 126 and y < 157.5 then 

      GraphicsWindow.PenColor = "red"

      GraphicsWindow.DrawLine(x2,y,x3,y)

    ElseIf y > 157.5 and y < 189 then 

      GraphicsWindow.PenColor = "white"

      GraphicsWindow.DrawLine(x2,y,x3,y)

    ElseIf y > 189 and y < 220.5 then 

      GraphicsWindow.PenColor = "red"

      GraphicsWindow.DrawLine(x2,y,x3,y)

    EndIf 

    '================================

  EndFor 

  

  '=========================================================

  'Bottom Part White/Red Stripes:

  ycount = 0 

  GraphicsWindow.PenColor = "white"

  For y = 220 To 441

    Program.Delay(1) 

    ycount = ycount + 0.5

    remains = Math.Remainder(ycount,31.5)

    ycount = ycount + 0.5    

    If remains = 0 Then 'when loop runs 31.5(220.5/7)times, triggers color change subroutine

      ycount = 0        'Reset control variable

      switchcolor()

    EndIf

    GraphicsWindow.DrawLine(x1,y,x3,y) 

  EndFor 

  '----------Include with each flag ---------------------------------------------------------------------

  CountryName = "USA"

  NameAndPause()

  '----------------------------------------------------------------------------------------------------------

  

EndSub 



Sub switchcolor 

  If count = 1 then

    GraphicsWindow.PenColor = "white"

    count = 0

  Else

    GraphicsWindow.PenColor = "red"

    count = 1 

  EndIf   

  

EndSub



