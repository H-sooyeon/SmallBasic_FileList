Sub Rainbow

  'rainbow

  x1 = 0 

  x2 = 624

  For y = 0 to 441

    Program.Delay(1)

    If y < 73.5 then 

      GraphicsWindow.PenColor = "red"

      GraphicsWindow.DrawLine(x1,y,x2,y)

    ElseIf y > 73.5 and y < 147 then 

      GraphicsWindow.PenColor = GraphicsWindow.GetColorFromRGB(229,187,0)

      GraphicsWindow.DrawLine(x1,y,x2,y) 

    elseif y > 147 and y < 220.5 then 

      GraphicsWindow.PenColor = "yellow"

      GraphicsWindow.DrawLine(x1,y,x2,y)

    elseif y > 220.5 and y < 294 then 

      GraphicsWindow.PenColor = "green"

      GraphicsWindow.DrawLine(x1,y,x2,y)

    elseif y > 294 and y < 367.5 then 

      GraphicsWindow.PenColor = "blue"

      GraphicsWindow.DrawLine(x1,y,x2,y)

    elseif y > 367.5 then  

      GraphicsWindow.PenColor = "purple"

      GraphicsWindow.DrawLine(x1,y,x2,y)

    EndIf 

  endfor 

  '----------Include with each flag ---------------------------------------------------------------------

  CountryName = "Rainbow"

  NameAndPause()

  '----------------------------------------------------------------------------------------------------------

EndSub



