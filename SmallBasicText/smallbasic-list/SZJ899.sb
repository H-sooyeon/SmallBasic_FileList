'Draw outline

For i = 1 To Array.GetItemCount(shape)

  GraphicsWindow.PenColor = shape[i]["color"]

  GraphicsWindow.DrawEllipse(shape[i]["x"], shape[i]["y"], shape[i]["Width"], shape[i]["height"])

  Program.Delay(300)

EndFor



'Add and Animate shapes for solid color

GraphicsWindow.PenWidth = 0

For i = 1 To Array.GetItemCount(shape)

  GraphicsWindow.BrushColor = shape[i]["color"]

  shp[i] = Shapes.AddEllipse(shape[i]["width"], shape[i]["height"])

  Shapes.Animate(shp[i], shape[i]["x"], shape[i]["y"], 500)

  Program.Delay(500)

EndFor












