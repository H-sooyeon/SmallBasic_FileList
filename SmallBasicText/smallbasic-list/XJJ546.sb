'  SB to VB sample   



GUI()



dx=800

dy=100

t0=clock.ElapsedMilliseconds

for k=1 to 2.3 step 0.005

  dx=dx-3

  dy=dy+1

  ScaleUp()

  Program.Delay(20)

endfor

GraphicsWindow.Title=(Clock.ElapsedMilliseconds-t0)+"  msec"





Sub ScaleUp

  For i=1 To Array.GetItemCount(dat)

    Shapes.Zoom(SHP[i],k,k)

    Shapes.Move(SHP[i],dx+dat[i]["x"]*k* s,dy+dat[i]["y"]*k* s)

  EndFor

EndSub





Sub GUI

  

  GraphicsWindow.width=Desktop.Width-100

  GraphicsWindow.height=Desktop.Height-100

  GraphicsWindow.BackgroundColor="Lightcyan"

  GraphicsWindow.Top=0

  GraphicsWindow.Left=0

  dat[1] = "fn=rc;x=60;y=61;w=52;h=88;bc=#222C3A;pc=#FFFFF3;pw=1;"  ' ?É¥ 

  dat[2] = "fn=el;x=62;y=11;w=48;h=58;bc=#222C3A;pc=#FFFFF3;pw=1;"   ' ?†≠

  dat[3] = "fn=rc;x=18;y=62;w=39;h=28;bc=#222C3A;pc=#FFFFF3;pw=1;"  'Â∑??Öï ‰∏? 

  dat[4] = "fn=rc;x=0;y=22;w=16;h=56;bc=#222C3A;pc=#FFFFF3;pw=1;"   ' Â∑??Öï ‰∏?

  dat[5] = "fn=tr;x=0;y=0;x1=0;y1=21;x2=16;y2=21;x3=6;y3=0;bc=#222C3A;pc=#FFFFF3;pw=1;"  'Â∑??ã≥  

  dat[6] = "fn=rc;x=58;y=146;w=19;h=70;bc=#222C3A;pc=#FFFFF3;pw=1;"  ' Â∑?Ë∂?

  dat[7] = "fn=el;x=47;y=206;w=31;h=19;bc=#222C3A;pc=#FFFFF3;pw=1;"  ' Â∑?Ë∂≥È¶ñ

  dat[8] = "fn=rc;x=108;y=61;w=39;h=28;bc=#222C3A;pc=#FFFFF3;pw=1;" ' ?è≥?Öï ‰∏?

  dat[9] = "fn=ln;x=142;y=22;x1=16;y1=0;x2=0;y2=56;pc=#222C3A;bc=#FFFFF3;pw=1;"??'  Á∑?  

  'dat[9] = "fn=rc;x=142;y=22;w=16;h=56;bc=#222C3A;pc=#FFFFF3;pw=1;"??'?è≥?Öï ‰∏?

  dat[10] = "fn=el;x=142;y=0;w=19;h=26;bc=#222C3A;pc=#FFFFF3;pw=1;"   '?è≥ ?ã≥  

  dat[11] = "fn=rc;x=84;y=146;w=19;h=70;bc=#222C3A;pc=#FFFFF3;pw=1;" ' ?è≥Ë∂?

  dat[12] = "fn=el;x=90;y=206;w=31;h=19;bc=#222C3A;pc=#FFFFF3;pw=1;" ' ?è≥Ë∂≥È¶ñ

  s=0.5

  For i = 1 To Array.GetItemCount(dat)

    GraphicsWindow.Penwidth = dat[i]["pw"]*s

    GraphicsWindow.PenColor = dat[i]["pc"]

    GraphicsWindow.BrushColor = dat[i]["bc"]

    If dat[i]["fn"] = "rc" Then

      SHP[i] = shapes.Addrectangle(dat[i]["w"]*s, dat[i]["h"]*s)

    ElseIf dat[i]["fn"] = "el" Then

      SHP[i] = shapes.Addellipse(dat[i]["w"]*s, dat[i]["h"]*s)

    ElseIf dat[i]["fn"] = "tr" Then

      SHP[i]= Shapes.Addtriangle(dat[i]["x1"]*s, dat[i]["y1"]*s, dat[i]["x2"]*s, dat[i]["y2"]*s, dat[i]["x3"]*s, dat[i]["y3"]*s)

    ElseIf dat[i]["fn"] = "ln" Then

      SHP[i]= Shapes.AddLine(dat[i]["x1"]*s, dat[i]["y1"]*s, dat[i]["x2"]*s, dat[i]["y2"]*s)

    EndIf

    shapes.Move(SHP[i], dat[i]["x"]*s, dat[i]["y"]*s)

    If Text.IsSubText("rc:el:tr", dat[i]["fn"]) And dat[i]["angle"] <> 0 Then

      Shapes.Rotate(SHP[i], dat[i]["angle"])

    EndIf

  EndFor

EndSub














