Sub bcc

  If Controls.LastClickedButton=bt1 Then

    For x=1 To w

      LDShapes.BrushColour(sh[x] "lightgray")

      LDShapes.PenWidth(sh[x] 2)

      LDShapes.PenColour(sh[x] "black")

    EndFor

  ElseIf Controls.LastClickedButton=bt3 Then

    For x=1 To w

      LDShapes.BrushColour(sh[x] "transparent")

      LDShapes.PenWidth(sh[x] .6)

      LDShapes.PenColour(sh[x] "cyan")

    EndFor

  ElseIf Controls.LastClickedButton=bts Then

    ee=LDControls.LastComboBoxIndex

    For x=1 To w

      If ee=1 Then

        LDEffect.Clear(sh[x])

      ElseIf ee=7 Then

        LDEffect.DropShadow(sh[x] "")

      ElseIf ee=2 Then

        LDEffect.Embossed(sh[x] "")

      ElseIf ee=3 Then

        LDEffect.Blur(sh[x] "")

      ElseIf ee=4 Then

        LDEffect.Monochrome(sh[x] "")

      ElseIf ee=5 Then

        LDEffect.Ripple(sh[x] "")

      ElseIf ee=6 Then

        LDEffect.Swirl(sh[x] "")

      EndIf

    EndFor

  Else

    For x=1 To w

      LDShapes.BrushColour(sh[x] clm[x])

      LDShapes.PenWidth(sh[x] 0)

    EndFor

  EndIf

EndSub





