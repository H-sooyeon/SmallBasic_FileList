Sub evv

  For y=2 To 7

    If li[y]="" Then

    Else

      li[y]=LDMath.Evaluate2(li[y]+"+0")

    EndIf

  EndFor

EndSub



Sub procs

  li=LDText.Split(args[1] " ")

  lii=li[1]

  qq=Text.GetCharacter(34)

  

  If lii="bgr" Then

    GraphicsWindow.BackgroundColor=li[2]

  ElseIf lii="pwd" Then

    GraphicsWindow.PenWidth=li[2]

    pww=li[2]

  ElseIf lii="pen" Then

    GraphicsWindow.PenColor=li[2]

  ElseIf lii="bcl" Then

    GraphicsWindow.BrushColor=li[2]

  ElseIf lii="ftr" or lii="dtr" Then

    evv()

    w=w+1

    clm[w]=GraphicsWindow.BrushColor

    sh[w]=Shapes.AddTriangle(li[2]-sx li[3]-sy li[4]-sx li[5]-sy li[6]-sx li[7]-sy)

    If lii="ftr" Then

      LDShapes.PenWidth(sh[w] 0)

    EndIf

    

  ElseIf lii="lin" Then

    evv()

    qll=Shapes.AddLine(li[2]-sx li[3]-sy li[4]-sx li[5]-sy)

    LDShapes.PenWidth(qll pww)

    

  ElseIf lii="fel" or lii="del" Then

    evv()

    If li[4]*li[5]>0 Then

      w=w+1

      clm[w]=GraphicsWindow.BrushColor

      sh[w]=Shapes.AddEllipse(li[4] li[5])

      Shapes.Move( sh[w] li[2]-sx li[3]-sy)  

      LDShapes.PenWidth(sh[w] 0)

    EndIf

  ElseIf lii="frc" or lii="drc" Then

    evv()

    If li[4]*li[5]>0 Then

      w=w+1

      clm[w]=GraphicsWindow.BrushColor

      sh[w]=Shapes.AddRectangle(li[4] li[5])

      Shapes.Move( sh[w] li[2]-sx li[3]-sy)

      LDShapes.PenWidth(sh[w] 0)

    EndIf

    

  Else 

    TextWindow.WriteLine(li)

  EndIf

EndSub


