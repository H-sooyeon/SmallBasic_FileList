'Create Print Font

Image = Printing.ScanImage()

If Image = "FAILED" Then

  Dialogs.ShowMessageBox("Scanning failed","","OK","Error")

  Program.End()

Else

  GameWindow.Show()

  GameControls.AddImage(Image,0,0)

EndIf


