GraphicsWindow.Title="Cherry Blossom"

Turtle.Speed=10

Turtle.Angle=-60

GraphicsWindow.PenColor="Pink"

For i = 1 To 5

  Turtle.Move(50)

  For a = 1 To 40

    Turtle.Move(1)

    Turtle.Turn(2)

  EndFor

  Turtle.Turn(45)

  For a = 1 To 15

    Turtle.Move(1)

  EndFor

  Turtle.Turn(-45)

  For a = 1 To 15

    Turtle.Move(1)

  EndFor

  Turtle.Turn(45)

  For a = 1 To 40

    Turtle.Move(1)

    Turtle.Turn(2)

  EndFor

  Turtle.Move(50)

  Turtle.Angle=-60+72*i

EndFor

Turtle.Hide()

LDShapes.RasteriseTurtleLines()

LDGraphicsWindow.FloodFill(315,225,"Pink")

GraphicsWindow.BrushColor="DeepPink"

GraphicsWindow.FillEllipse(320,210,65,65)

GraphicsWindow.FontName="Times New Roman"

GraphicsWindow.FontItalic="true"

GraphicsWindow.FontSize=24

GraphicsWindow.DrawText(50,50,"?™©Sakura, Japanese folk song")

Sound.PlayMusic("O6 A4 A4 B2 A4 A4 B2 A4 B4 O7 C4 O6 B4 A4 B8 A8 F2 E4 C4 E4 F4 E4 E8 C8 O5 B2 O6 A4 B4 O7 C4 O6 B4 A4 B8 A8 F2 E4 C4 E4 F4 E4 E8 C8 O5 B2 O6 A4 A4 B2 A4 A4 B2 E4 F4 B8 A8 F4 E1.5")'"Sakura"


