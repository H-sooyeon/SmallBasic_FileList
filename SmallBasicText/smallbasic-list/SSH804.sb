' add eyelids
GraphicsWindow.PenColor = "Blue
GraphicsWindow.BrushColor = "Blue
For i = 1 To 2
  eyelids[i] = Shapes.AddEllipse(90, 80)
  Shapes.move(eyelids[i], 0, 0)
  Shapes.animate(eyelids[i], 200 + (i - 1) * 140, 197, 2000)
EndFor

Timer.Tick = Blink
Timer.Interval = 500

Sub Blink
  t = t + 1
  If Math.Remainder(t, 6) = 1 Then
    For i = 1 To 2
      Shapes.HideShape(eyelids[i])
    EndFor
  ElseIf Math.Remainder(t, 6) = 0 Then
    For i = 1 To 2
      Shapes.ShowShape(eyelids[i])
    EndFor
  EndIf
EndSub

