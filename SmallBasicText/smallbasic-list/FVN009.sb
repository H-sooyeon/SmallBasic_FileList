numbers()

loop()

GraphicsWindow.DrawText(0,0,num1)

GraphicsWindow.DrawText(0,30,num2)

GraphicsWindow.DrawText(0,60,num3)

GraphicsWindow.DrawText(0,90,"Highest number is: " + max)





Sub numbers

num1 = Math.GetRandomNumber(100)

num2 = Math.GetRandomNumber(100)

num3 = Math.GetRandomNumber(100)

EndSub

Sub loop

If num1 < num2 Then

  max = num2 

Else 

  max = num1

EndIf

If num3 < max Then

  max = max

Else 

  max = num3

EndIf

EndSub



  


