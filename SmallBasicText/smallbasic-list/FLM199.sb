sun=ImageList.LoadImage("http://www.wikispaces.com/file/view/504px-Sol_de_Mayo-Bandera_de_Argentina_svg.png/33732763/504px-Sol_de_Mayo-Bandera_de_Argentina_svg.png")

Shapes.AddImage(sun)
Shapes.Zoom(sun,0.2,0.2)

line=Shapes.AddLine(10,10,30,50)


angle=0
while "True"
  
  Shapes.Rotate(sun,angle)
  shapes.rotate(line,angle)
  
  angle=angle+5
  
  Program.Delay(100)
  EndWhile
