SUB Read_Measure
  
  If (M=1) then ' measure 1
    
      
 n[1]="O6L6 E"
 n[2]="O6L6 D#"

 
 ElseIf (M=2) then ' measure 2
 
 n[1]="O6L6 E"
 n[2]="O6L6 D#"
 n[3]="O6L6 E" 
 n[4]="O5L6 B"
 n[5]="O6L6 D" ' becarre
 n[6]="O6L6 C"

 ElseIf (M=3) then ' measure 3
  
 a[1]="O3L64 A" +"O5L64 A" 
 n[1]="O5L6 A"
 n[2]="O4L6 E"
 n[3]="O4L6 A"
 
 n[4]="O5L6 C"
 n[5]="O5L6 E"
 n[6]="O5L6 A"
 
ElseIf (M=4) then ' measure 4
  
  a[1]="O3L64 E" +"O5L64 B" 
  n[1]="O5L6 B"
  n[2]="O4L6 E"
  n[3]="O4L6 G#"
 
 n[4]="O5L6 E"
 n[5]="O5L6 G#"
 n[6]="O5L6 B"
 
 
ElseIf (M=5) then ' measure 5
  
  a[1]="O3L64 A" +"O5L64 C" 
  n[1]="O5L6 C"
  n[2]="O4L6 E"
  n[3]="O4L6 A"
 
 n[4]="O5L6 E"
 n[5]="O6L6 E"
 n[6]="O6L6 D#"
 
 ElseIf (M=6) then ' measure 6
  
n[1]="O6L6 E"
 n[2]="O6L6 D#"
 n[3]="O6L6 E" 
 n[4]="O5L6 B"
