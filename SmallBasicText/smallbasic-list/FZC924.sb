 n[6]="O6L6 C"
  
ElseIf (M=7) then ' measure 7
  
 a[1]="O3L64 A" +"O5L64 A" 
 n[1]="O5L6 A"
 n[2]="O4L6 E"
 n[3]="O4L6 A"
 
 n[4]="O5L6 C"
 n[5]="O5L6 E"
 n[6]="O5L6 A"
 
ElseIf (M=8) then ' measure 8
  
a[1]="O3L64 E" +"O5L64 B" 
  n[1]="O5L6 B"
