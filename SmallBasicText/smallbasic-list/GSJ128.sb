TextWindow.Title = "Student Scores"
TextWindow.WriteLine("How many grades will be calculated?")
numberOfStudents = TextWindow.ReadNumber()
a = 1

While (a <= numberOfStudents)
  
TextWindow.WriteLine("What is Student Percentage " + a)
studentScorePercentage = TextWindow.ReadNumber()
If studentScorePercentage > 75 Then
  TextWindow.WriteLine("A")
ElseIf studentScorePercentage >= 60 then
  TextWindow.WriteLine("B")
elseif  studentScorePercentage >= 35 then
  TextWindow.WriteLine("C")
else
  TextWindow.WriteLine("D")
EndIf
a = a + 1
EndWhile
