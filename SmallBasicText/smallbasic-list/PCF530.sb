    Sub Menu

      Sound.Play(Program.Directory+"/Level1.mp3")

'Add and Move Images For Menu              

BackGround=Shapes.AddImage(Program.Directory+"/Menu.png")

SuperManText=Shapes.AddImage(Program.Directory+"/SuperManText.png")

SuperMan_Menu=Shapes.AddImage(Program.Directory+"/SuperMan_Menu.png")

'Graphics Window Settings For Menu

GraphicsWindow.Width=1024

GraphicsWindow.Height=768

GraphicsWindow.CanResize="False"

GraphicsWindow.FontName="Algerian"

GraphicsWindow.FontSize="80"

StartButton=Controls.AddButton("Start",GraphicsWindow.Width/2-300,GraphicsWindow.Height/2-100)

EndButton=Controls.AddButton("End",GraphicsWindow.Width/2-300,GraphicsWindow.Height/2+100)

Controls.SetSize(StartButton,600,100)

Controls.SetSize(EndButton,600,100)

While Controls.LastClickedButton<>StartButton And Controls.LastClickedButton<>EndButton  

 EndWhile

 If Controls.LastClickedButton=EndButton Then

   Program.End()

   EndIf  

'Clear Screen Before Entering Other SubRoutines

Controls.HideControl(StartButton)

Controls.HideControl(EndButton)

Shapes.HideShape(BackGround)

Shapes.HideShape(SuperManText)

Shapes.HideShape(SuperMan_Menu)

EndSub 

'Summarize the levels in a subroutine



    Sub Level1

        SetUpGraphicsWindow()

        SetUpVariables()

        SetUpImages()        

        DrawImages_Level1()

        SpawnSuperMan_1()

        Listen_1()

        KeyDown_1()

        IsSuperManInPortal()                  

      EndSub

      Sub Level2       

        HideImages()

        DrawImages_Level2()

        SpawnSuperMan_2()

        SpawnGoro()

        Listen_2()

        GoroCutScene()

      EndSub

      Sub Level3

        SetUpGraphicsWindow_3()

        HideImages()

        DrawImages_Level3()

        SpawnSuperMan_3()        

        SpawnMotaro()

        Listen_3()

        MotaroCutScene()

        AddHitCircle()

        MotaroListener()

        EndSub

        'Run When SuperMan Wins Game

        Sub SuperManWins

          TextWindow.WriteLine("CONGRATULATIONS YOU HAVE BEATEN THE SUPERMAN GAME AND DESTROYED THE TWO VILLANOUS BOSSES")

          Program.Delay(1000)

          Program.End()

        EndSub

        'Runs When Motaro Wins

        Sub MotaroWins

          TextWindow.WriteLine("YOU HAVE LOST DUE TO THE UNBEARABLE UGLINESS OF MOTARO")

          Program.Delay(1000)

          Program.End()

          EndSub

        'Used to do nothing to disable certain commands

        Sub Nothing

        EndSub

      'Play the whole game

      Sub Game                   

          'Level2:

          If Level1_Status="Done" And Level2_Status<>"Done" Then

            Level2()

          ElseIf Level1_Status="Incomplete" Then

            Level1()

          ElseIf Level2_Status="Done"Then

            Level3()

            EndIf            

           EndSub

        'Run Commands

        Menu()

        SetUpVariables()

        Game()

                      




