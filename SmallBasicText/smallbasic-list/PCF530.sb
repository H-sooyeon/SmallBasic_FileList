  'Procedings of listeners

Sub KeyDown_1  

  

  'Get Shape Cordinates

 GetSuperMansCordinates()

  'Set the Last Key to a variable

  GetLastKey()

    'Run a subroutine depeding on the last key  

    If LastKey="W" Then

    LastKey_W_1()

    IsSuperManInPortal()

  ElseIf LastKey="S"Then

    LastKey_S_1()

    IsSuperManInPortal()

  ElseIf LastKey="A" Then

    LastKey_A_1()

    IsSuperManInPortal()

 ElseIf  LastKey="D" Then

   LastKey_D_1()

   IsSuperManInPortal()

    EndIf

  EndSub

  Sub KeyDown_2

    GetLastKey()

    If LastKey="D" And SuperManActive="True" Then

      Direction_2="Right"

      Altitude_2="Ground"

      LastKey_D_2()

    ElseIf LastKey="A" And SuperManActive="True" Then

      Direction_2="Left"

      Altitude_2="Ground"

      LastKey_A_2()

    ElseIf LastKey="W" And SuperManActive="True" Then

      Altitude_2="Up"

      LastKey_W_2()

    ElseIf LastKey="Space" And SuperManActive="True" And Level2_Status="Done" Then

      LastKey_Space_2()

      'Motaro's Actions

      EndIf

     EndSub

    

'Move SuperMan, Proceedings of If Statements

Sub LastKey_W_1

    CurrentCharacter=Character_Up

     Shapes.HideShape(Character_Left)

    Shapes.HideShape(Character_Right)

    Shapes.HideShape(Character_Down)

    Shapes.ShowShape(CurrentCharacter)

    Shapes.Move(CurrentCharacter,x,y-10)   

  EndSub  

  Sub LastKey_S_1

    CurrentCharacter=Character_Down

     Shapes.HideShape(Character_Left)

    Shapes.HideShape(Character_Right)

    Shapes.HideShape(Character_Up)

    Shapes.ShowShape(CurrentCharacter)    

    Shapes.Move(CurrentCharacter,x,y+10)

  EndSub  

  Sub LastKey_A_1

    CurrentCharacter=Character_Left

    Shapes.HideShape(Character_Right)

    Shapes.HideShape(Character_Down)

    Shapes.HideShape(Character_Up)

    Shapes.ShowShape(CurrentCharacter)

    Shapes.Move(CurrentCharacter,x-10,y)

  EndSub  

  Sub LastKey_D_1    

    CurrentCharacter=Character_Right

    Shapes.HideShape(Character_Left)

    Shapes.HideShape(Character_Down)

    Shapes.HideShape(Character_Up)

    Shapes.ShowShape(CurrentCharacter)

    Shapes.Move(CurrentCharacter,x+10,y)

  EndSub

  Sub LastKey_A_2

If Goro="Dead" And Level2_Status<>"Done" Then

IsSuperManInPortal_2()

EndIf

    BorderSuperMan_2()

    'Hide All Right Frames And SuperMan Floating

    For i = 1 To 8

      Shapes.HideShape(SuperManPunching_Left_[i])

      Shapes.HideShape(SuperManPunching_Right_[i])

        Shapes.HideShape(SuperManWalking_Right_[i])  

        Shapes.HideShape(SuperManFloating_Left_[i])

      Shapes.HideShape(SuperManFloating_Right_[i])

        

  EndFor

      'Counter Frame

      Frame_Left=Frame_Left+1

      'SetCurrentCharacterToFrame

      CurrentCharacter=SuperManWalking_Left_[Frame_Left]

      'HideLastImage

       If Frame_Left = 1 Then

        Shapes.HideShape(SuperManWalking_Left_[8])

        Else

          Shapes.HideShape(SuperManWalking_Left_[Frame_Left-1])

          EndIf

        'ShowNewImage

        Shapes.ShowShape(SuperManWalking_Left_[Frame_Left])      

        'MoveSuperManRight

        x=x-FootDistance

        Shapes.Move(CurrentCharacter,x,y)  

      'ResetFrame

      If Frame_Left=8 Then 

        Frame_Left=0

      EndIf        

    EndSub

    Sub LastKey_D_2

             If Goro="Dead" And Level2_Status<>"Done" Then

         IsSuperManInPortal_2()

EndIf

      BorderSuperMan_2()

  'Hide All Left Frames And SuperMan Floating

  For i = 1 To 8

      Shapes.HideShape(SuperManPunching_Left_[i])

      Shapes.HideShape(SuperManPunching_Right_[i])    

      Shapes.HideShape(SuperManWalking_Left_[i])

      Shapes.HideShape(SuperManFloating_Left_[i])

      Shapes.HideShape(SuperManFloating_Right_[i])

  EndFor  

  'Counter Frame

  Frame_Right=Frame_Right+1

  'SetCurrentCharacterToFrame

  CurrentCharacter=SuperManWalking_Right_[Frame_Right]

    'HideLastImage

   If Frame_Right = 1 Then

      Shapes.HideShape(SuperManWalking_Right_[8])

      Else

        Shapes.HideShape(SuperManWalking_Right_[Frame_Right-1])

        EndIf

      'ShowNewImage

      Shapes.ShowShape(SuperManWalking_Right_[Frame_Right])      

      'MoveSuperManRight

      x=x+FootDistance

      Shapes.Move(CurrentCharacter,x,y)    

      'ResetFrame

    If Frame_Right=8 Then 

  Frame_Right=0

EndIf

EndSub

Sub LastKey_W_2  

  'Hide All Other Frames

    For i = 1 To 8    

      Shapes.HideShape(SuperManWalking_Left_[i])

      Shapes.HideShape(SuperManWalking_Right_[i])

      Shapes.HideShape(SuperManPunching_Left_[i])

      Shapes.HideShape(SuperManPunching_Right_[i])

    EndFor    

      If Direction_2="Right" Then       

       'Counter Frame

      Frame_Up_Right=Frame_Up_Right+1

      'SetCurrentCharacterToFrame

      CurrentCharacter=SuperManFloating_Right_[Frame_Up_Right]

      'MoveSuperManFloatingLeft

      Shapes.Move(CurrentCharacter,x,y_Floating)

      'HideLastImage

       If Frame_Up_Right = 1 Then

      Shapes.HideShape(SuperManFloating_Right_[4])

      Else

        Shapes.HideShape(SuperManFloating_Right_[Frame_Up_Right-1])

      EndIf

      'ShowNewImage

      Program.Delay(100)

      Shapes.ShowShape(CurrentCharacter) 

       'ResetFrame

    If Frame_Up_Right=4 Then 

      Frame_Up_Right=0          

EndIf   

    Else       

      'Counter Frame

      Frame_Up_Left=Frame_Up_Left+1

      'SetCurrentCharacterToFrame

      CurrentCharacter=SuperManFloating_Left_[Frame_Up_Left]

      'MoveSuperManFloatingLeft

      Shapes.Move(CurrentCharacter,x,y_Floating)

      'HideLastImage

       If Frame_Up_Left = 1 Then

