columnwidth = 10

columnheight = 80



GraphicsWindow.Width = columnwidth*100

GraphicsWindow.Height = columnheight*3

GraphicsWindow.PenWidth = 0



For col = 0 To 49

  x = col*columnwidth*2

  GraphicsWindow.BrushColor = "Magenta"

  GraphicsWindow.FillRectangle(x,0,columnwidth,columnheight)

  GraphicsWindow.BrushColor = "Cyan"

  GraphicsWindow.FillRectangle(x+columnwidth,0,columnwidth,columnheight)

EndFor

For col = 0 To 9

  x = col*columnwidth*2

  GraphicsWindow.BrushColor = "Cyan"

  GraphicsWindow.FillRectangle(x,columnheight,columnwidth,columnheight)

  GraphicsWindow.BrushColor = "Orange"

  GraphicsWindow.FillRectangle(x+columnwidth,columnheight,columnwidth,columnheight)

EndFor

For col = 10 To 39

  x = col*columnwidth*2

  GraphicsWindow.BrushColor = "Magenta"

  GraphicsWindow.FillRectangle(x,columnheight,columnwidth,columnheight)

  GraphicsWindow.BrushColor = "Orange"

  GraphicsWindow.FillRectangle(x+columnwidth,columnheight,columnwidth,columnheight)

EndFor

For col = 40 To 49

  x = col*columnwidth*2

  GraphicsWindow.BrushColor = "Magenta"

  GraphicsWindow.FillRectangle(x,columnheight,columnwidth,columnheight)

  GraphicsWindow.BrushColor = "Cyan"

  GraphicsWindow.FillRectangle(x+columnwidth,columnheight,columnwidth,columnheight)

EndFor

For col = 0 To 49

  x = col*columnwidth*2

  GraphicsWindow.BrushColor = "Cyan"

  GraphicsWindow.FillRectangle(x,columnheight*2,columnwidth,columnheight)

  GraphicsWindow.BrushColor = "Orange"

  GraphicsWindow.FillRectangle(x+columnwidth,columnheight*2,columnwidth,columnheight)

EndFor


