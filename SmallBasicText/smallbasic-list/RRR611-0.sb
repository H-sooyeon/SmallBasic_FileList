Sub DrawStars

  GraphicsWindow.BrushColor = "White"

  For i = 1 To 500

    x = Math.GetRandomNumber(gw) - 1

    y = Math.GetRandomNumber(gh) - 1

    s = Math.GetRandomNumber(3)

    GraphicsWindow.FillEllipse(x, y, s, s)

  EndFor

EndSub



Sub GenerateRandomArray

  ra[0] = 1

  ra[1] = 0 ' end

  n = 2

  For i = 2 To 25

    p = 0

    o = Math.GetRandomNumber(n) - 1

    While 0 < o

      p = ra[p]

      o = o - 1

    EndWhile

    ra[i] = ra[p]

    ra[p] = i

    n = n + 1

  EndFor

EndSub



