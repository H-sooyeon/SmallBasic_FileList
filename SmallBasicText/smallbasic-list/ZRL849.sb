GraphicsWindow.Title="Republic of China(1912-1928)"
GraphicsWindow.Width=640
GraphicsWindow.Height=400
gh=400
gw=640
gh5=gh/5
bc[1]="#de2119"
bc[2]="#ffc500"
bc[3]="#08639c"
bc[4]="#ffffff"
bc[5]="#000000"
For i = 1 To 5
  GraphicsWindow.BrushColor=bc[i]
  GraphicsWindow.FillRectangle(0,(i-1)*gh5,gw,gh5)
EndFor
