Sub comp_turn_normal
  If matches<=0 Then
    TextWindow.Clear()
    TextWindow.CursorLeft = 25
    TextWindow.WriteLine("The computer won! Maybe next time.")
    post_game()
  Else
    show_matches()
    If matches > 3 Then
      comp_matches_removed = 3
    ElseIf matches = 3 then
      comp_matches_removed = 2
    ElseIf matches = 2 then
      comp_matches_removed = 1
    elseif matches = 1 then
      comp_matches_removed = 1
    EndIf
    
    TextWindow.WriteLine("")
    TextWindow.CursorLeft = 27
    TextWindow.WriteLine("It is the computer's turn.")
    TextWindow.WriteLine("")
    TextWindow.CursorLeft = 17
    TextWindow.WriteLine("The computer removes " + comp_matches_removed + " match(es) from the table.")
    matches = matches - comp_matches_removed
    TextWindow.WriteLine("")
    TextWindow.CursorLeft = 27
    TextWindow.Pause()
    player_turn="true"
    comp_turn="false"
    player_turn_sub()
  EndIf
EndSub

Sub comp_turn_hard
  If matches<=0 Then
    TextWindow.Clear()
    TextWindow.CursorLeft = 25
    TextWindow.WriteLine("The computer won! Maybe next time.")
    post_game()
  Else
    show_matches()
    If matches - 13 > 3 And matches > 0 Then
      comp_matches_removed = 3
    ElseIf matches - 13 <= 3 and matches - 13 > 0 then
      comp_matches_removed = matches - 13
    elseIf matches-9 > 3 and matches>0 Then
      comp_matches_removed = 3
    ElseIf matches - 9 <= 3 and matches - 9 > 0 then
      comp_matches_removed = matches - 9
    elseif matches < 9 and matches - 5 > 3  and matches > 0 then
      comp_matches_removed = 3
    elseif matches < 9 and matches - 5 <= 3 and matches-5 > 0 then
      comp_matches_removed = matches - 5
    elseif matches = 5 then
      comp_matches_removed = 2
    elseif matches = 4 then
      comp_matches_removed = 3
    ElseIf matches = 3 then
      comp_matches_removed = 2
    ElseIf matches = 2 then
      comp_matches_removed = 1
    elseif matches = 1 then
      comp_matches_removed = 1
    EndIf
    
    TextWindow.WriteLine("")
    TextWindow.CursorLeft = 27
    TextWindow.WriteLine("It is the computer's turn.")
    TextWindow.WriteLine("")
    TextWindow.CursorLeft = 17
    TextWindow.WriteLine("The computer removes " + comp_matches_removed + " match(es) from the table.")
    matches = matches - comp_matches_removed
    TextWindow.WriteLine("")
    TextWindow.CursorLeft = 27
    TextWindow.Pause()
    player_turn="true"
    comp_turn="false"
    player_turn_sub()
  EndIf
EndSub

