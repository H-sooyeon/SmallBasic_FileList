Sub post_game
  TextWindow.CursorLeft = 27
  TextWindow.Pause()
  TextWindow.Clear()
  TextWindow.CursorLeft = 23
  TextWindow.WriteLine("Would you like to play again?(Y or N)")
  TextWindow.WriteLine("")
  TextWindow.CursorLeft = 40
  continue= TextWindow.Read()
  If continue<>"Y" And continue<>"N" And continue <> "y" And continue <> "n" Then
    TextWindow.WriteLine("")
    TextWindow.CursorLeft = 25
    TextWindow.WriteLine("Let's try that again...")
    TextWindow.WriteLine("")
    post_game()
  EndIf
  If continue = "Y" Or continue = "y" then
    matches = 15
    game_start()
  elseif continue="n" or continue="N" then
    Program.End()
  EndIf
  
EndSub

