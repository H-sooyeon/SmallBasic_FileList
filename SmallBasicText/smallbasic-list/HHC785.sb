Sub easy
  won_the_game="false"
  show_matches()
  turn = Math.GetRandomNumber(2)
  
  If turn=1 Then
    
    TextWindow.WriteLine("")
    
    comp_turn="true"
    player_turn="false"
    
  ElseIf turn = 2 then
    
    TextWindow.WriteLine("")
    TextWindow.CursorLeft = 30
    TextWindow.WriteLine("You will go first.")
    TextWindow.WriteLine("")
    comp_turn="false"
    player_turn="true"
    
  EndIf
  
  If player_turn="true" Then
    player_turn_sub()
  ElseIf comp_turn="true" then
    comp_turn_easy()
  EndIf
EndSub  


Sub comp_turn_easy
  If matches<=0 Then
    TextWindow.Clear()
    TextWindow.CursorLeft = 30
    TextWindow.WriteLine("The computer won! Maybe next time.")
    post_game()
  Else
    show_matches()
    If matches >= 3 Then
      comp_matches_removed = Math.GetRandomNumber(3)
    ElseIf matches = 2 then
      comp_matches_removed = Math.GetRandomNumber(2)
    elseif matches = 1 then
      comp_matches_removed = 1
    EndIf
    
    TextWindow.WriteLine("")
    TextWindow.CursorLeft = 27
    TextWindow.WriteLine("It is the computer's turn.")
    TextWindow.WriteLine("")
    TextWindow.CursorLeft = 17
    TextWindow.WriteLine("The computer removes " + comp_matches_removed + " match(es) from the table.")
    matches = matches - comp_matches_removed
    TextWindow.WriteLine("")
    TextWindow.CursorLeft = 27
    TextWindow.Pause()
    player_turn="true"
    comp_turn="false"
    player_turn_sub()
  EndIf
EndSub



