'CLICK

GraphicsWindow.MouseDown = click

Sub click

  x = GraphicsWindow.MouseX

  y = GraphicsWindow.MouseY

  

  random = Math.GetRandomNumber(3)

  

  getanswer1 = Controls.GetTextBoxText(tb)

  

  getnum = Controls.GetButtonCaption(num)

  

  'PLAY

  If getnum = 0 Then

    If x > 5 And x < 155 And y > 135 And y < 205 Then

      Controls.SetButtonCaption(num, 1)

      Sound.PlayBellRing()

      Shapes.HideShape(playel)

      Shapes.HideShape(helpel)

      Shapes.HideShape(exitel)

      Shapes.HideShape(play)

      Shapes.HideShape(help)

      Shapes.HideShape(exit)

      Shapes.HideShape(menuback)

      

      'QUESTION_ONE_1

      GraphicsWindow.FontSize = 100

      GraphicsWindow.BrushColor = "yellow"

      a = Math.GetRandomNumber(50)

      b = Math.GetRandomNumber(50)

      a1 = Shapes.AddText(a)

      Shapes.Move(a1, 100, 100)

      GraphicsWindow.BrushColor = "lime"

      b1 = Shapes.AddText(b)

      Shapes.Move(b1, 550, 100)

      Shapes.ShowShape(tbback)

      Controls.ShowControl(next1)

      Controls.ShowControl(tb)

      For i = 1 To 100

        Shapes.SetOpacity(leftarrow1, i)

        Shapes.SetOpacity(leftarrow2, i)

        Shapes.SetOpacity(leftarrow3, i)

        Shapes.SetOpacity(rightarrow1, i)

        Shapes.SetOpacity(rightarrow2, i)

        Shapes.SetOpacity(rightarrow3, i)

      EndFor

      getleft1 = Shapes.GetLeft(leftarrow1)

      gettop1 = Shapes.GetTop(leftarrow1)

      getleft2 = Shapes.GetLeft(leftarrow2)

      gettop2 = Shapes.GetTop(leftarrow2)

      getleft3 = Shapes.GetLeft(leftarrow3)

      gettop3 = Shapes.GetTop(leftarrow3)

      gettop11 = Shapes.GetTop(rightarrow1)

      getleft11 = Shapes.GetLeft(rightarrow1)

      getleft22 = Shapes.GetLeft(rightarrow2)

      getleft33 = Shapes.GetLeft(rightarrow3)

      gettop22 = Shapes.GetTop(rightarrow2)

      gettop33 = Shapes.GetTop(rightarrow3)

      Shapes.Animate(leftarrow1, getleft1 + 200, gettop1 + 0, 150)

      Shapes.Animate(leftarrow2, getleft2 + 200, gettop2 + 0, 300)

      Shapes.Animate(leftarrow3, getleft3 + 200, gettop3 + 0, 300)

      Shapes.Animate(rightarrow1, getleft11 - 200, gettop11 + 0, 150)

      Shapes.Animate(rightarrow2, getleft22 - 200, gettop22 + 0, 300)

      Shapes.Animate(rightarrow3, getleft33 - 200, gettop33 + 0, 300)

      For i = 1 To 100

        Shapes.SetOpacity(enteranswer, i)

        Program.Delay(5)

      EndFor

      If random = 1 Then

        Shapes.ShowShape(plus)

        Sound.PlayClick()

        Controls.SetButtonCaption(calculation, 1)

      Else

        If random = 2 Then

          Shapes.ShowShape(minus)

          Sound.PlayClick()

          Controls.SetButtonCaption(calculation, 2)

        Else

          If random = 3 Then

            Shapes.ShowShape(multiplication)

            Sound.PlayClick()

            Controls.SetButtonCaption(calculation, 3)

          EndIf

        EndIf

      EndIf

    EndIf

    

    'HELP

    If getnum = 0 Then

      If x > 5 And x < 155 And y > 235 And y < 305 Then

        Controls.SetButtonCaption(num, 1)

        Sound.PlayClick()

        getleftmenuback = Shapes.GetLeft(menuback)

        getleftplayel = Shapes.GetLeft(playel)

        getlefthelpel = Shapes.GetLeft(helpel)

        getleftexitel = Shapes.GetLeft(exitel)

        getleftplay = Shapes.GetLeft(play)

        getlefthelp = Shapes.GetLeft(help)

        getleftexit = Shapes.GetLeft(exit)

        gettopmenuback = Shapes.GetTop(menuback)

        gettopplayel = Shapes.GetTop(playel)

        gettophelpel = Shapes.GetTop(helpel)

        gettopexitel = Shapes.GetTop(exitel)

        gettopplay = Shapes.GetTop(play)

        gettophelp = Shapes.GetTop(help)

        gettopexit = Shapes.GetTop(exit)

        Shapes.Animate(menuback, getleftmenuback - 200, gettopmenuback, 200)

        Shapes.Animate(playel, getleftplayel - 200, gettopplayel, 50)

        Shapes.Animate(helpel, getlefthelpel - 200, gettophelpel, 100)

        Shapes.Animate(exitel, getleftexitel - 200, gettopexitel, 100)

        Shapes.Animate(play, getleftplay - 200, gettopplay, 50)

        Shapes.Animate(help, getlefthelp - 200, gettophelp, 100)

        Shapes.Animate(exit, getleftexit - 200, gettopexit, 100)

        Shapes.ShowShape(help1)

        Shapes.ShowShape(help2)

        Shapes.ShowShape(help3)

        Shapes.ShowShape(help4)

        Shapes.ShowShape(help5)

        Shapes.ShowShape(help6)

        Shapes.ShowShape(help7)

        Shapes.ShowShape(help8)

        Shapes.ShowShape(help9)

        Controls.ShowControl(ok)

      EndIf

    EndIf

  EndIf

  

  'EXIT

  If getnum = 0 Then

    If x > 5 And x < 155 And y > 335 And y < 405 Then

      Controls.SetButtonCaption(num, 1)

      Sound.PlayClickAndWait()

      Program.End()

    EndIf

  EndIf

EndSub



