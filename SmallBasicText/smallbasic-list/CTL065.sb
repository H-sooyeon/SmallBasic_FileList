Sub texttyped

  Shapes.HideShape(enteranswer)

  Shapes.HideShape(leftarrow1)

  Shapes.HideShape(leftarrow2)

  Shapes.HideShape(leftarrow3)

  Shapes.HideShape(rightarrow1)

  Shapes.HideShape(rightarrow2)

  Shapes.HideShape(rightarrow3)

EndSub



