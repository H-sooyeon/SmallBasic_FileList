GraphicsWindow.Title = "International Code of Signals"

gw = GraphicsWindow.Width

gh = GraphicsWindow.Height

bg = "tan"

GraphicsWindow.BackgroundColor = bg

fw = 60

fh = 40



Init()

For k = 1 To 26

  

  LDCall.Function3("ICS" Text.GetSubText(alpha, k, 1) 1 1)

  Program.Delay(5)

  CC[K]=LDGraphicsWindow.Capture("" "faLSE")

  Program.Delay(5)

  LDImage.Crop(cc[k] 0 0 fw fh)

  GraphicsWindow.Clear()

EndFor

For k = 1 To 26

  ii=Shapes.AddImage(cc[k])

  LDShapes.SetShapeEvent(ii)

  If k>2 Then

    LDEffect.DropShadow(ii "")

  EndIf

  

  LDDialogs.ToolTip(ii Text.GetCharacter(64+k))

  rrr:

  rx=Math.GetRandomNumber(600)

  ry=Math.GetRandomNumber(500)

  

  ac= array.GetItemCount( LDShapes.GetAllShapesAt(rx ry))

  ac=ac+ array.GetItemCount( LDShapes.GetAllShapesAt(rx+fw ry))

  ac=ac+ array.GetItemCount( LDShapes.GetAllShapesAt(rx ry+fh))

  ac=ac+ array.GetItemCount( LDShapes.GetAllShapesAt(rx+fw ry+fh))

  

  If ac>0 Then

    Goto rrr

  EndIf

  Shapes.Animate(ii rx ry 200)

  Program.Delay(222)

EndFor

args=0

LDShapes.ShapeEvent=see

GraphicsWindow.MouseMove=mmww

GraphicsWindow.MouseUp=muu

LDEvents.MouseWheel=mwhh



Sub ICS

  flag=args[1]

  x = args[2]

  y = args[3]

  _x=x

  _y=y

  If flag = "A" Then

    bc = "White"

    ldcall.Function2 ("FRect" fh/2 fh)

    x = x + width

    width = fw - width

    height = fh / 2

    bc = "Blue"

    vertex = "bottom"

    FillTriangle()

    y = y + height

    vertex = "top"

    FillTriangle()

    y = y - height

    ldcall.Function2 ("FRect" width/2 fh)

    

  ElseIf flag = "B" Then

    bc = "Red"

    width = fw / 2 + fh / 4

    ldcall.Function2 ("FRect" width fh)

    x = x + fh / 2

    width = fw - fh / 2

    height = fh / 2

    vertex = "bottom"

    FillTriangle()

    y = y + height

    vertex = "top"

    FillTriangle()

    

  ElseIf flag = "C" Then

    bc = "Blue"

    height = fh

    ldcall.Function2 ("FRect" fw fh)

    bc = "White"

    y = y + fh / 5

    height = fh * 3 / 5

    ldcall.Function2 ("FRect" fw height)

    bc = "Red"

    y = y + fh / 5

    height = fh / 5

    ldcall.Function2 ("FRect" fw height)

    

  ElseIf flag = "D" Then

    bc = "Yellow"

    ldcall.Function2 ("FRect" fw fh)

    bc = "Blue"

    y = y + fh / 5

    height = fh * 3 / 5

    ldcall.Function2 ("FRect" fw height)

    

  ElseIf flag = "E" Then

    bc = "Blue"

    ldcall.Function2 ("FRect" fw fh)

    bc = "Red"

    y = y + fh / 2

    height = fh / 2

    ldcall.Function2 ("FRect" fw height)

    

  ElseIf flag = "F" Then

    bc = "White"

    width = fw

    ldcall.Function2 ("FRect" fw fh)

    bc = "Red"

    vertex = "top"

    height = fh / 2

    FillTriangle()

    vertex = "bottom"

    y = y + height

    FillTriangle()

    

  ElseIf flag = "G" Then

    bc = "Yellow"

    height = fh

    ldcall.Function2 ("FRect" fw fh)

    bc = "Blue"

    width = fw / 6

    x = x - width

    For i = 1 To 3

      x = x + width * 2

      ldcall.Function2 ("FRect" width height)

    EndFor

    

  ElseIf flag = "H" Then

    bc = "White"

    ldcall.Function2 ("FRect" fw fh)

    bc = "Red"

    x = x + fw

    ldcall.Function2 ("FRect" fw/2 fh)

    

  ElseIf flag = "I" Then

    bc = "Yellow"

    width = fw

    height = fh

    ldcall.Function2 ("FRect" width height)

    bc = "Black"

    size = fh * 0.6

    ox = x + fw / 2

    oy = y + fh / 2

    FillCircle()

    

  ElseIf flag = "J" Then

    bc = "Blue"

    ldcall.Function2 ("FRect" fw fh)

    bc = "White"

    y = y + fh / 3

    height = fh / 3

    ldcall.Function2 ("FRect" width height)

    

  ElseIf flag = "K" Then

    bc = "Yellow"

    ldcall.Function2 ("FRect" fw fh)

    bc = "Blue"

    width = fw / 2

    x = x + width

    ldcall.Function2 ("FRect" width fh)

    

  ElseIf flag = "L" Then

    bc = "Yellow"

    ldcall.Function2 ("FRect" fw fh)

    bc = "Black"

    width = fw / 2

    height = fh / 2

    x = x + width

    ldcall.Function2 ("FRect" width height)

    x = x - width

    y = y + height

    ldcall.Function2 ("FRect" width height)

    

  ElseIf flag = "M" Then

    bc = "White"

    ldcall.Function2 ("FRect" fw fh)

    bc = "Blue"

    x = x + w * fw

    width = (1 - 2 * w) * fw

    height = (0.5 - w) * fh

    vertex = "bottom"

    FillTriangle()

    vertex = "top"

    y = y + fh - height

    FillTriangle()

    x = _x

    y = _y + w * fh

    width = (0.5 - w) * fw

    height = (1 - 2 * w) * fh

    vertex = "right"

    FillTriangle()

    vertex = "left"

    x = _x + fw - width

    FillTriangle()

    

  ElseIf flag = "N" Then

    bc = "Blue"

    width = fw

    height = fh

    ldcall.Function2 ("FRect" width height)

    bc = "White"

    width = fw / 4

    height = fh / 4

    fill = "False"

    For j = 1 To 4

      y = _y + (j - 1) * height

      For i = 1 To 4

        If fill Then

          x = _x + (i - 1) * width

          ldcall.Function2 ("FRect" width height)

        EndIf

        fill = Not[fill]

      EndFor

      fill = Not[fill]

    EndFor

    

  ElseIf flag = "O" Then

    bc = "Yellow"

    width = fw

    height = fh

    ldcall.Function2 ("FRect" width height)

    bc = "Red"

    vertex = "topright"

    FillTriangle()

    

  ElseIf flag = "P" Then

    bc = "Blue"

    width = fw

    height = fh

    ldcall.Function2 ("FRect" fw fh)

    bc = "White"

    x = x + fw / 3

    y = y + fh / 3

    width = fw / 3

    height = fh / 3

    ldcall.Function2 ("FRect" width height)

    

  ElseIf flag = "Q" Then

    bc = "Yellow"

    ldcall.Function2 ("FRect" fw fh)

    

  ElseIf flag = "R" Then

    bc = "Red"

    width = fw

    ldcall.Function2 ("FRect" width fh)

    bc = "Yellow"

    y = y + fh * 2 / 5

    height = fh / 5

    ldcall.Function2 ("FRect" width height)

    width = fh / 5

    height = fh

    x = _x + (fw - width) / 2

    y = _y

    ldcall.Function2 ("FRect" width height)

    

  ElseIf flag = "S" Then

    bc = "White"

    width = fw

    height = fh

    ldcall.Function2 ("FRect" fw fh)

    bc = "Blue"

    x = x + fw / 3

    y = y + fh / 3

    ldcall.Function2 ("FRect" fw/3 fh/3)

    

  ElseIf flag = "T" Then

    bc = "Red"

    ldcall.Function2 ("FRect" fw fh)

    bc = "White"

    x = x + fw / 3

    width = fw / 3

    ldcall.Function2 ("FRect" width fh)

    bc = "Blue"

    x = x + fw / 3

    ldcall.Function2 ("FRect" width fh)

    

  ElseIf flag = "U" Then

    bc = "Red"

    ldcall.Function2 ("FRect" fw fh)

    bc = "White"

    width = fw / 2

    height = fh / 2

    x = x + width

    ldcall.Function2 ("FRect" width height)

    x = x - width

    y = y + height

    ldcall.Function2 ("FRect" width height)

    

  ElseIf flag = "V" Then

    bc = "Red"

    ldcall.Function2 ("FRect" fw fh)

    bc = "White"

    x = _x + w * fw

    y = y - 1

    width = (1 - 2 * w) * fw

    height = (0.5 - w) * fh

    vertex = "bottom"

    FillTriangle()

    vertex = "top"

    y = _y + fh - height

    FillTriangle()

    x = _x

    y = _y + w * fh

    width = (0.5 - w) * fw

    height = (1 - 2 * w) * fh

    vertex = "right"

    FillTriangle()

    vertex = "left"

    x = _x + fw - width

    FillTriangle()

    

  ElseIf flag = "W" Then

    bc = "Blue"

    ldcall.Function2 ("FRect" fw fh)

    bc = "White"

    x = x + fw / 5

    y = y + fh / 5

    width = fw * 3 / 5

    height = fh * 3 / 5

    ldcall.Function2 ("FRect" width height)

    bc = "Red"

    x = x + fw / 5

    y = y + fh / 5

    width = fw / 5

    height = fh / 5

    ldcall.Function2 ("FRect" width height)

    

  ElseIf flag = "X" Then

    bc = "White"

    ldcall.Function2 ("FRect" fw fh)

    bc = "Blue"

    y = y + fh * 2 / 5

    height = fh / 5

    ldcall.Function2 ("FRect" fw height)

    width = fh / 5

    height = fh

    x = _x + (fw - width) / 2

    y = _y

    ldcall.Function2 ("FRect" width height)

    

  ElseIf flag = "Y" Then

    bc = "Yellow"

    ldcall.Function2 ("FRect" fw fh)

    bc = "Red"

    vertex = "bottomright"

    width=fw

    height=fh

    FillTriangle()

    For i = 4 To 1 Step -1

      width = fw * i / 5

      height = fh * i / 5

      If Math.Remainder(i, 2) = 0 Then

        bc = "Red"

      Else

        bc = "Yellow"

      EndIf

      x = _x

      y = _y

      vertex = "topleft"

      FillTriangle()

      If Math.Remainder(i, 2) = 0 Then

        bc = "Yellow"

      Else

        bc = "Red"

      EndIf

      x = _x + fw * (5 - i) / 5

      y = _y + fh * (5 - i) / 5

      vertex = "bottomright"

      FillTriangle()

    EndFor

    

  ElseIf flag = "Z" Then

    bc = "Yellow"

    height = fh

    ldcall.Function2 ("FRect" fw height)

    bc = "Black"

    vertex = "right"

    width = fw / 2

    FillTriangle()

    bc = "Blue"

    vertex = "left"

    x = _x + width

    FillTriangle()

    bc = "Red"

    vertex = "top"

    width = fw

    height = fh / 2

    x = _x

    y = _y + height

    FillTriangle()

  EndIf

EndSub



Sub muu

  sss=""

EndSub



Sub mmww

  If sss="" Then

  Else

    Shapes.Move(sss GraphicsWindow.MouseX GraphicsWindow.MouseY)

  EndIf

EndSub



Sub mwhh

  If sss="" Then

  Else

    rrr[sss]=rrr[sss]+LDEvents.LastMouseWheelDelta*3

    Shapes.Rotate(sss rrr[sss])

  EndIf

  

EndSub



Sub see

  If LDShapes.LastEventType="MouseDown" Then

    sss=LDShapes.LastEventShape

  EndIf

EndSub



Sub FillCircle

  GraphicsWindow.BrushColor = bc

  GraphicsWindow.FillEllipse(ox - size / 2, oy - size / 2, size, size)

EndSub



Sub FRect

  GraphicsWindow.BrushColor = bc

  GraphicsWindow.FillRectangle(x, y, args[1] args[2])

EndSub



Sub FillTriangle

  GraphicsWindow.BrushColor = bc

  If vertex = "top" Then

    x1 = x + width / 2

    y1 = y

    x2 = x

    y2 = y + height

    x3 = x + width

    y3 = y2

  ElseIf vertex = "topleft" Then

    x1 = x

    y1 = y

    x2 = x + width

    y2 = y

    x3 = x

    y3 = y + height

  ElseIf vertex = "topright" Then

    x1 = x + width

    y1 = y

    x2 = x

    y2 = y

    x3 = x + width

    y3 = y + height

  ElseIf vertex = "bottom" Then

    x1 = x + width / 2

    y1 = y + height

    x2 = x

    y2 = y

    x3 = x + width

    y3 = y2

  ElseIf vertex = "bottomright" Then

    x1 = x + width

    y1 = y + height

    x2 = x + width

    y2 = y

    x3 = x

    y3 = y + height

  ElseIf vertex = "left" Then

    x1 = x

    y1 = y + height / 2

    x2 = x + width

    y2 = y

    x3 = x + width

    y3 = y + height

  ElseIf vertex = "right" Then

    x1 = x + width

    y1 = y + height / 2

    x2 = x

    y2 = y

    x3 = x

    y3 = y + height

  EndIf

  GraphicsWindow.FillTriangle(x1, y1, x2, y2, x3, y3)

EndSub



Sub Init

  Not = "False=True;True=False;"

  alpha = "ABCDEFGHIJKLMNOPQRSTUVWXYZ"

  

  w = 0.1

EndSub


