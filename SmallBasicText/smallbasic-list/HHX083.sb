ldcall.function4 ("ell" 470, 325, 40, 35)
GraphicsWindow.BrushColor = "#1F6915"
ldcall.function4 ("ell" 450, 325, 40, 35)
LDEvents.MouseWheel=mww
GraphicsWindow.KeyDown=kkk
zz=1
args=0

Sub ell
  If args[3]*args[4]>0 then
    ss=shapes.AddEllipse (args[3] args[4])
    ' Program.Delay (5)
    Shapes.Move (ss args[1] args[2])
    LDShapes.PenWidth (ss 0)
  EndIf 
EndSub 

Sub ell1
  If args[3]*args[4]>0 then
    ss=shapes.AddEllipse (args[3] args[4])
    ' Program.Delay (5)
    Shapes.Move (ss args[1] args[2])
    LDShapes.BrushColour  (ss "transparent")
  EndIf 
EndSub 
Sub rct
  If args[3]*args[4]>0 then
    ss=shapes.AddRectangle  (args[3] args[4])
    ' Program.Delay (5)
    Shapes.Move (ss args[1] args[2])
    LDShapes.PenWidth (ss 0)
  EndIf
endsub

Sub mww
  
  zz=zz+ LDEvents.LastMouseWheelDelta/10
  
  ldGraphicsWindow.Reposition (zz zz xx yy 0)
EndSub 

Sub kkk
  
  ls=text.ConvertToLowerCase (GraphicsWindow.LastKey) 
  If ls="left" then
    xx=xx-5
  ElseIf ls="right" then
    xx=xx+5
  ElseIf ls="up" then
    yy=yy-5
  ElseIf ls="down" then
    yy=yy+5
    
  EndIf 
  ldGraphicsWindow.Reposition (zz zz xx yy 0)
EndSub 

