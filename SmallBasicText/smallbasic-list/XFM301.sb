    If var[5] = 6 then
      red()
    EndIf
    TextWindow.WriteLine("Reset Entire Order")
    bottomlist()
  EndIf
  intro = TextWindow.Readkey()
  If intro = 8 and var[5] = 0  and var[2] = 0 then
    var[5] = 4
    introclear()
  elseIf var[2] = 1 and intro = 8 and var[5] = 0 then
    var[5] = 6
