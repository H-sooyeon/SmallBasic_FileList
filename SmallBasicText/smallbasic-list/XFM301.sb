  ElseIf intro = 8 and var[5] = 1 then
    var[5] = 0
    introclear()
  elseIf  intro = 8 then
    var[5] = var[5] - 1
    introclear()
  ElseIf intro = 5 and var[5] = 4 and var[2] = 0 then
    var[5] = 0
    introclear()
  ElseIf intro = 5 and var[5] = 4 and var[2] = 1 then
    var[5] = 5
    introclear()
  ElseIf intro = 5 and var[5] = 6 then
    var[5] = 0
    introclear()
  ElseIf intro = 5 then
    var[5] = var[5] + 1
    introclear()
  ElseIf intro = "+" then
    if var[5] = 0 then
      mainstreetentreesclear()
    ElseIf  var[5] = 1 then
      atasteofthewildsidestillinthehideclear()
    ElseIf  var[5] = 2 then
      eatingfoodismorefunwhenyouknowitwashitontherunclear()
    ElseIf  var[5] = 3 then
      roadkillcafeLateNightDelightsclear()
    ElseIf var[5] = 4 then
      roadkillcafemenuchallengersclear()
    ElseIf  var[5] = 5 and var[2] = 1 then
      orderconfirmationclear()
    ElseIf  var[5] = 6 and var[2] = 1 then
      quantities()
      var[2] = 0
      var[5] = 0
      introclear()
    Else
      introclear()
    EndIf
  Else
    introclear()
  EndIf
