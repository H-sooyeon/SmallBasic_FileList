GraphicsWindow.Title="Flower?"

GraphicsWindow.BackgroundColor="LightSkyBlue"

Turtle.Speed=10

GraphicsWindow.PenColor="White"

For h = 1 To 10

  Turtle.PenUp()

  Turtle.MoveTo(320+(h-1)*5,240+(h-1)*5)

  Turtle.Angle=-60

  Turtle.PenDown()

  For i = 1 To 5

    Turtle.Move(50)

    For j = 1 To 100

      Turtle.Move(1-(h-1)*0.1)

      Turtle.Turn(2)

    EndFor

    Turtle.Move(50-(h-1)*5)

    Turtle.Angle=-60+72*i

  EndFor

EndFor

Turtle.Hide()


