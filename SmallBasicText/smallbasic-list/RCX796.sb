      character = Text.GetCharacter(character/e_numhelp2)

      textboxtext = Text.Append(textboxtext, character)

    EndFor

    FCControls.SetText(textbox, textboxtext)

  Else

    GraphicsWindow.ShowMessage("Your password is incorrect", "ERROR!!! NOOO!!!!")

  EndIf

