Sub intro
  fairytailpic1 = Shapes.AddImage(Fairytailpic)
  Shapes.Move(fairytailpic1, 150, -168)
  Shapes.Animate(fairytailpic1, 150, 116, 4000)
  Program.Delay(4000)
