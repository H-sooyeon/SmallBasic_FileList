    If player_magic_energy >= player_magic_energy_limit then
      player_magic_energy = player_magic_energy_limit
    Else
      player_magic_energy = player_magic_energy + 200
      If player_magic_energy >= player_magic_energy_limit then
        player_magic_energy = player_magic_energy_limit
      endif
