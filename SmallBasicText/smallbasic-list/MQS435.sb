Sub fight 
  grassbattlefield()
  character() ' loads the character gui
  enemy()  'loads enemy gui
  enemyattack = Math.GetRandomNumber(3)
  turn = 1
  action:
  While enemyhealth > 0 and player_health > 0
    display()
    Shapes.Remove(enemyhealthpic)
    enemydisplay()
    If player_magic_type= "Fire"Then
      fireanimation()
    ElseIf player_magic_type= "Water" then
      wateranimation()
    elseif player_magic_type = "Wind" then
      windanimation()
    EndIf
    probabilityofhitting = Math.Abs(speed/(speed+enemyspeed) * 100)
    TextWindow.WriteLine("Probability of hitting for player = " + probabilityofhitting)
    
    probabilitynum = Math.GetRandomNumber(100)
    TextWindow.WriteLine("Your number was " + probabilitynum)
    If turn > 1 then ' if turn is now going again
      If magic = 2 Or magic = 3 Then ' this is only relavent when the fight repeats because eaction is not zero anymore
        enemyhasdefence()
      else 
        enemyhasoffence()
      endif
    Else 
      If probabilitynum < probabilityofhitting Or probabilitynum = probabilityofhitting Then
        If player_magic_type = "Water" or player_magic_type = "Wind" then
          If actionr = 1 and player_magic_energy >= 100Then ' this is the process to enemy losing health
            enemyhealth = enemyhealth - player_magic_strength
          ElseIf actionr = 2 and player_magic_energy >= 200  and player_level >= 7 then
            enemyhealth = enemyhealth
          ElseIf actionr = 3 and player_magic_energy >= 300 and player_level >= 30 then
            enemyhealth = enemyhealth - (player_magic_strength * 3)
          Else 
            Goto action ' redos actions if player does not choose a valid response
          EndIf
        Else 
          If actionr = 1 and player_magic_energy >= 100 Then ' this is the process to enemy losing health
            enemyhealth = enemyhealth - player_magic_strength
          ElseIf actionr = 2 and player_magic_energy >= 200  and player_level >= 7 then
            enemyhealth = enemyhealth - (player_magic_strength * 2)
          ElseIf actionr = 3 and player_magic_energy >= 300 and player_level >= 30 then
            enemyhealth = enemyhealth - (player_magic_strength * 3)
          Else 
            Goto action ' redos actions if player does not choose a valid response
          EndIf
        endif
      endif
      'if missed then do nothing
    EndIf
    
    TextWindow.WriteLine("Enemy health is " + enemyhealth)
    
    display() ' remakes display
    If enemyhealth < 0 Or enemyhealth = 0 Then 'checks to see if you one ko the enemy
      Goto skip
    EndIf
    
    'does enemy probablility now
    probabilityofhitting = Math.Abs(enemyspeed/(speed+enemyspeed) * 100)
    TextWindow.WriteLine("Probablility of enemy hitting is = " + probabilityofhitting)
    ' if the enemy speed is 90 and your speed is 100, enemy has a 2 percent chance of hitting you
    probabilitynum = Math.GetRandomNumber(100)
    TextWindow.WriteLine("Enemy probability was " + probabilitynum)
    If magic = 1 then
      enemyfire() ' enemy turn
      TextWindow.WriteLine("enemy action was " +eaction)
    ElseIf magic = 2 then 
      enemywater()
      TextWindow.WriteLine("enemy action was " +eaction)
    elseif magic = 3 then
      enemywind()
      TextWindow.WriteLine("enemy action was " +eaction)
    endif
    
    If player_magic_type = "Water" Or player_magic_type = "Wind" then ' player has a defence magic and probably a defence move
      playerhasdefence()
    Else 'player does not have a defence magic
      playerhasoffence()
    endif
    
    enemydisplay() ' shows enemy display
    
    skip:
    TextWindow.WriteLine("Player health is " + player_health)
    If enemyhealth < 0 Then
      GraphicsWindow.ShowMessage("You won", "Player won")
      display()
      expearnfromenemy()
    ElseIf player_health < 0 then 
      GraphicsWindow.ShowMessage("You lost", "Player lost")   
    EndIf 
    turn = turn + 1 
  EndWhile
  player_health = player_level_health
  Shapes.Remove(ehead)
  Shapes.Remove(eleg1)
  Shapes.Remove(eleg2)
  Shapes.Remove(emarm)
  Shapes.Remove(elarm)
  Shapes.Remove(etorso)
  display()
  
endsub
