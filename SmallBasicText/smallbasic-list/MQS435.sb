Sub playerhasdefence ' player has a defence magic type
  If probabilitynum < probabilityofhitting Or probabilitynum = probabilityofhitting then
    If magic = 2 or magic = 3 Then 'enemy has a defence magic
      If eaction <> 2 and actionr = 2 Then 'if  enemy chose an attack move and player choose a defence move then
        player_health = player_health ' 
      ElseIf eaction <> 2 and actionr <> 2 then 'if enemy chose an attack move and player did not choose a defence move then
        If eaction = 1 Then
          player_health = player_health - 100
        elseif eaction = 3 then  
          player_health = player_health - 300
        EndIf 
      Else 'enemy chose a defence move
        player_health = player_health
      EndIf
    Else ' enemy does not have a defence magic
      If actionr = 2 Then ' if player chose a defence move
        player_health = player_health
      Else 'player didnt choose a defence move
        
        If eaction = 1 Then ' enemy attack
          player_health = player_health - 100
        ElseIf eaction = 2 then  
          player_health = player_health - 200
        elseif eaction = 3 then  
          player_health = player_health - 300
        EndIf
      endif 
    EndIf
  Else 
    TextWindow.WriteLine("Enemy Missed")
  endif
endsub
Sub playerhasoffence ' player has a attack magic
  If probabilitynum < probabilityofhitting Or probabilitynum = probabilityofhitting then
    If magic = 2 or magic = 3 Then 'enemy has a defence magic type
      If eaction = 2 Then 'enemy chose a defence move
        'do nothing
      Else 'enemy has chosen a attack move
        If eaction = 1 Then
          player_health = player_health - 100
        Else
          player_health = player_health - 300
        endif
      EndIf
    Else ' enemy does not have a defence magic
      If eaction = 1 Then
        player_health = player_health - 100
      ElseIf eaction = 2 then  
        player_health = player_health - 200
      elseif eaction = 3 then  
        player_health = player_health - 300
      EndIf 
    endif
  Else 
    TextWindow.WriteLine("Enemy Missed")
  endif
EndSub
