        Program.Delay(50)
      endfor
      Shapes.Remove(marm)
      marm = Shapes.AddLine(110, 245, 120, 245)
      TextWindow.Pause()
      expearn()
    Else
      TextWindow.WriteLine("you do not have enough energy (300) or you need to be level 30 or higher")
    endif
