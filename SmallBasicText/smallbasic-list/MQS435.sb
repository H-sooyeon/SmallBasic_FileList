Sub Tries
  GraphicsWindow.BrushColor = "Black" 
  TextWindow.WriteLine("you have: " + tries + " tries left")
  magicnumber = Math.GetRandomNumber(3) ' There is only fire magic.
  If magicnumber = 3 Then
    magicnumber = magicnumber + 1
  endif
  If magicnumber = 1 Then
    player_magic_type = "Fire"
    player_move1 = "Fireball"
    player_move2 = "Fire Bullets"
    player_move3 = "Brilliant Flame"
  ElseIf magicnumber = 2 Then
    player_magic_type = "Water"
    player_move1 = "Water Lock"
    player_move2 = "Water Wall"
    player_move3 = "Tsunami"
  Elseif magicnumber = 3 Then 
    player_magic_type = "Earth"
    player_move1 = "Earth Bullets"
    player_move2 = "Earth Wall"
    player_move3 = "Earth King Rock Crush"
  Elseif magicnumber = 4 Then
    player_magic_type = "Wind" 
    player_move1 = "Wind Shurikens"
    player_move2 = "Wind Shell"
    player_move3 = "Storm Bringer"
  Elseif magicnumber = 5 then
    player_magic_type = "Poison Dragon Slayer"
    player_move1 = "Roar of the Poison Dragon"
    player_move2 = "Poison Claws"
    player_move3 = "Poison Chamber"
  ElseIf magicnumber = 6 Then
    player_magic_type = "Acid"
    player_move1 = "Acid Bazooka"
    player_move2 = "Acid Bullets"
    player_move3 = "Ultimate Acid"
  Elseif magicnumber = 7 then
    player_magic_type = "Lighning"
    player_move1 = "Lighning Bolt"
    player_move2 = "Lighning Shockwave"
    player_move3 = "Lightning Storm"
  Elseif magicnumber = 8 then
    player_magic_type = "Ice"
    player_move1 = "Ice Hammer"
    player_move2 = "Ice Cannon"
    player_move3 = "ice Shell"
  elseif magicnumber = 9 then
    player_magic_type = "Death"
    player_move1 = "Death Orb"
    player_move2 = "Death Current"
    player_move3 = "Death Grab"
  Elseif magicnumber = 10 then
    player_magic_type = "Bomb"
    player_move1 = "Grenade"
    player_move2 = "Time Bomb"
    player_move3 = "Self-Destruction"
  Endif
  TextWindow.WriteLine("Your magic type is " + player_magic_type)
  TextWindow.WriteLine("Do you like your magic?")
  answer = TextWindow.Read()
  If answer = "no" Then
    tries = tries - 1
    TextWindow.Clear()
    Tries()
  ElseIf answer = "yes" then
    TextWindow.WriteLine("your moves are: " + player_move1 + ". As you level up you will gain more moves.")
    player_level = 1
    player_level_health = 100
    player_health = 100
    player_magic_type = player_magic_type
    player_strength = 50
    player_magic_strength = 100
    magic_energy_take = 100
    player_magic_energy = 100
    player_exp = 0
    player_exp_level = 1
    player_magic_energy_limit = 100
    speed = 1
  EndIf
endsub
