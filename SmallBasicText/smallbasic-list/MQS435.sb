      Shapes.Remove(marm)
      marm = Shapes.AddLine(110, 245, 135, 270)
      player_magic_energy = player_magic_energy - 300
      GraphicsWindow.BrushColor = "Yellow"
      brilliant_flame = Shapes.AddEllipse(10,10)
