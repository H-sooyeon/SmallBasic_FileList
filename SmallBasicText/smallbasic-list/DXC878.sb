' Permutation (with duplications or not)



Not = "False=True;True=False;"

m = 3

allowDup = "True"

i = 1

Node()



Sub Node

  For c = 1 To m

    If allowDup Or Not[Array.ContainsValue(n, c)] Then

      n[i] = c

      If i = m Then

        Print()

      ElseIf i < m Then

        i = i + 1

        Stack.PushValue("local", c)

        Node()

        c = Stack.PopValue("local")

        i = i - 1

      EndIf

      n[i] = ""

    EndIf

  EndFor

EndSub



Sub Print

  TextWindow.Write("(")

  For _i = 1 To m

    If 1 < _i Then

      TextWindow.Write(",")

    EndIf

    TextWindow.Write(n[_i])

  EndFor

  TextWindow.WriteLine(")")

EndSub


