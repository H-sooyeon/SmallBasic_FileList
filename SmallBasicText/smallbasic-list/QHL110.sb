x = 0.6 

ArcTan()

TextWindow.WriteLine(y)



Sub ArcTan

  If x > 1 then

    x = 1 / x

    ArcTanInternal()

    y = Math.Pi / 2 - y

  Else

    ArcTanInternal()

  EndIf

EndSub



Sub ArcTanInternal

  y = x - Math.Power(x, 3) / 3 + Math.Power(x, 5) / 5 - Math.Power(x, 7) / 7

EndSub




