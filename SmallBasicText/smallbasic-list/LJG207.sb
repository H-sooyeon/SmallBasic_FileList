'======================================================================================================



Sub GetDirection

  If Angto < 0 Then 

    Angto = Angto + 360

  EndIf

  If Angto > 360 Then 

    Angto = Angto - 360

  EndIf

  Var1 =  (360 - AngAt) -  (360 - Angto)

  

  If Var1 <-180 Then

    Var1 = Var1 + 360

  EndIf

  If Var1 > 180 Then

    Var1 = Var1 - 360

  EndIf

  

  If Var1 > 0 Then

    Dir[UserNum] = 1

  EndIf

  If Var1 < 0 Then

    Dir[UserNum] = -1

  EndIf

EndSub



'======================================================================================================



Sub RefineAngle

  If AngleInput < 0 Then 

    AngleInput = AngleInput + 360

  EndIf

  If AngleInput > 360 Then 

    AngleInput = AngleInput - 360

  EndIf

  AngleOutput = AngleInput

EndSub



