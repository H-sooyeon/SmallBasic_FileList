Sub SendServerCheck

  ElapsedMilisecs = Math.Round(Clock.ElapsedMilliseconds)

  Info4["T"] = "CH"

  Info4["N"] = UserNum

  Info4["TS"] = ElapsedMilisecs

  LDClient.SendMessage(Info4)

EndSub



