Sub OnKeyDown

  For KeydownNum = 1 To Array.GetItemCount(Keybind)

    If GraphicsWindow.LastKey = Keybind[KeydownNum] Then

      Key[KeydownNum] = "Down"

    EndIf

  EndFor

EndSub



'======================================================================================================



Sub OnKeyUp

  For KeyUpNum = 1 To Array.GetItemCount(Keybind)

    If GraphicsWindow.LastKey = Keybind[KeyUpNum] Then

      Key[KeyUpNum] = "Up"

    EndIf

  EndFor

EndSub



