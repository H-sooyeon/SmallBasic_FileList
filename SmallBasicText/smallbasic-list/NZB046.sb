zz=LDText.Split("# ? $ : [ ] > < = + - * / ` , . '" " ")



cc=LDGraphicsWindow.Capture("" "false")

Program.Delay(777)

LDImage.Crop(cc 10 10 480+240 480)

ii=LDImage.Copy(cc)

LDImage.ReplaceColour(cc "tan" "transparent" 10)

LDImage.ReplaceColour(ii "darkblue" "transparent" 10)

LDImage.ReplaceColour(ii "tan" "darkblue"  10)



ch=LDImage.SplitImage(cc 9 6)

ci=LDImage.SplitImage(ii 9 6)

LDImage.SaveAs(cc "e:\zxc1.png")

LDImage.SaveAs(ii "e:\zxc11.png")



GraphicsWindow.Width=900

GraphicsWindow.Clear()



For x=0 To 54

  If x<10 Then

    chr["L"+text.GetCharacter( 48+x)]=cI [Math.Floor(x/6)+1][Math.Remainder(x 6)+1]

    chr[text.GetCharacter( 48+x)]=ch[Math.Floor(x/6)+1][Math.Remainder(x 6)+1]

  ElseIf x<36 Then

    chr["l"+text.GetCharacter( 55+x)]=ci[Math.Floor(x/6)+1][Math.Remainder(x 6)+1]

    chr[text.GetCharacter( 55+x)]=ch[Math.Floor(x/6)+1][Math.Remainder(x 6)+1]

  Else

    chr["l"+zz[x-35]]=ci[Math.Floor(x/6)+1][Math.Remainder(x 6)+1]

    chr[zz[x-35]]=ch[Math.Floor(x/6)+1][Math.Remainder(x 6)+1]

    

  EndIf

EndFor

