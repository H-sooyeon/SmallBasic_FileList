ss="10 input a$~!20 print !abcde_fghij!klmno_12345 ~30 for !<i> = 1 to 10.2~!40 print 'i# * + [5-4]/2$,5`3'~50 next !i

dx=0

x=1

dy=20



While x<=Text.GetLength(ss)

  While dx<32

    cc=Text.GetSubText(ss x 1)

    If cc="!" Then

      pf="l"

    ElseIf cc=" " Then

      pf=""

      dx=dx+1

    ElseIf cc="_" Then

      pf=""

    ElseIf cc="~" Then

      dx=32

      Goto xxx

    Else

      dx=dx+1

    EndIf

    GraphicsWindow.DrawResizedImage( chr[pf+cc] dx*30 dy 30 30)

    xxx:

    x=x+1

  EndWhile

  dy=dy+30

  dx=0

EndWhile


