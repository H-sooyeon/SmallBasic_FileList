Sub Boundries

  If Area = "RougePort" Then

   'ROUGEPORT BOUNDRIES

   '=============================

   If MarioY < 450 Then

     MarioY = 460

   EndIf

   If MarioY > 675 Then

     MarioY = 665

   EndIf

   If MarioX < 390 And MarioY < 495 Then

      MarioX = 400

   EndIf

