        Coins = Coins - 15

        

        Program.Delay(1000)

        Elseif GraphicsWindow.MouseX > 70 and GraphicsWindow.MouseX < 500 and GraphicsWindow.MouseY > 355 and GraphicsWindow.MouseY < 375 and Coins >= 15 then

        AmtPOWBlock = AmtPOWBlock + 1

        Coins = Coins - 15

        

        Program.Delay(1000)

