Sub BlackJackScoring

  If BlackJackTotal <= 21 Then

    BlackJackJudge()

  ElseIf BlackJackTotal > 21 Then

    GraphicsWindow.BrushColor = "Red"

    GraphicsWindow.FontSize = 30

    GraphicsWindow.DrawText(1000, 375, "Computer Wins!")

  EndIf

EndSub

Sub BlackJackJudge

  If BlackJackTotal > ComputerTotal Then

    GraphicsWindow.BrushColor = "Red"

    GraphicsWindow.FontSize = 40

    GraphicsWindow.DrawText(1000, 375 "Player Wins!")

  ElseIf ComputerTotal > BlackJackTotal Then

    GraphicsWindow.BrushColor = "Red"

    GraphicsWindow.FontSize = 40

    GraphicsWindow.DrawText(1000, 375 "Computer Wins!")

  Else

    GraphicsWindow.BrushColor = "Red"

    GraphicsWindow.FontSize = 40

    GraphicsWindow.DrawText(1000, 375 "Tie!")

  EndIf

EndSub

Sub FaceValue

  If CardValue = 10 Or CardValue + 11 Or CardValue = 12 Then

    CardValue = 9

  ElseIf CardValue = 13 Then

    CardValue = 10

  EndIf

EndSub

