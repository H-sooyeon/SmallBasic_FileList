'Main Subroutines---------------------------------------------------------------------------------------------------------------------------------------------

Sub CardDisplay

CardNumber = Math.GetRandomNumber(52)

If(CardNumber>= 1 and CardNumber <=13) then

  Suit = "Hearts"

  CardValue = CardNumber

ElseIf (CardNumber >=14 and CardNumber <=26) then

  Suit = "Diamonds"

  CardValue = CardNumber - 13

ElseIf (CardNumber >=27 and CardNumber <=39) then

Suit = "Clubs"

  CardValue = CardNumber - 26

Else

  Suit = "Spades"

  CardValue = CardNumber - 39

EndIf

EndSub

Sub GetSuit

  If Suit = "Hearts" Then

    GraphicsWindow.BrushColor = "Red"

    GraphicsWindow.PenColor = "Red"

    GraphicsWindow.FillEllipse((CardX + 40), (CardY + 125), 75, 75)

    GraphicsWindow.FillEllipse((CardX + 90), (CardY + 125), 75, 75)

    GraphicsWindow.FillTriangle ((CardX + 43), (CardY + 175), (CardX + 100), (CardY + 250), (CardX + 163), (CardY + 175))

  EndIf

