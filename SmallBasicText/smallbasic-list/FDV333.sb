Sub OnMouseMove

   paddleX = GraphicsWindow.MouseX - player_size/2

   paddleY = GraphicsWindow.MouseY- player_size



If (paddleX < 0) then

   paddleX = 0

endif

   Shapes.Move(player, paddleX , paddleY )

 EndSub

