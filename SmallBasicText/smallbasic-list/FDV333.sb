' Main game routine

Sub Play

   GraphicsWindow.Show()



   ' Main loop

   play = 1

   pause = 0

   incx =0   ' X-axis increment for background

   incy = 0  ' Y-axis increment for background

   incbx = 0  'increment for bullets and all objects on X

   incby = 0 'increment for bullets and all objects on Y

   'wave = 1

' Planet initial position 

j= 0

   For i= 1 To planetcount

     j=Math.Remainder(i,6)

     posy[i]= planetPos[j][2]

     posx[i]= planetPos[j][3]

     incplanetx[i] = 0

     incplanety[i] = 0

   EndFor

   TimePlay = 0

   Shapes.Move(player, gameWidth/2 , gameHeight -80 )

   While(play = 1)

     Program.Delay(1000/fps)

     TimePlay = TimePlay +1

     ''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

     If Player_Lives< 1 Then

       'itsgameover ="True"

       play=0

           EndIf

     '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

     If (pause = 0) Then

       If (TimePlay = level1[wave][1]) then



         pathNBR = level1[wave][2]

         alienPosX1 = level1[wave][3]  'X position

         alienPosY1 = level1[wave][4]  'Y position

         alien_Nbr = level1[wave][6]

         alien_type = level1[wave][7]

         if (level1[wave][5]=1) then

           create_aliens_left()

         Else

           create_aliens_right()

         endif

         wave = wave + 1

       endif

       

       If (TimePlay > 4000) Then

           Shapes.Move(Shapes.addImage(end), 230,200)

           Program.Delay(2000)

         Goto thisshouldendit

        Endif

       

       moveall()



       GraphicsWindow.MouseMove = OnMouseMove

       GraphicsWindow.MouseDown = OnMouseDown



       AgePlayer_Ammo()



EndIf



EndWhile



thisshouldendit:

 

EndSub '''End of Sub Play

