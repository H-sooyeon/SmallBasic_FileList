  Sub Collision_pbe   'for player-bullet and aliens



    For i1 = 1 To Player_AmmoCount

    ' player bullet position

     Player_Ammox = Shapes.GetLeft(Array.GetValue(Player_Ammo, i1))

     Player_Ammoy = Shapes.GetTop(Array.GetValue(Player_Ammo, i1))

     px1=Player_Ammox+player_bullet_size/3   'more accurate than bullet size

     py1=Player_Ammoy+player_bullet_size/3

     px2=px1+2*player_bullet_size/3

     py2=py1+2*player_bullet_size/3



        For i2 = 1 to alien_Count

         ' alien position 



         ax1=Array.GetValue(alien_PosX,i2)+alien_size/4

         ay1=Array.GetValue(alien_PosY,i2)+alien_size/4



         ax2=ax1+3*alien_size/4

         ay2=ay1+3*alien_size/4





         If ( (ax1 < px1 And ax2 > px1) Or (ax1 < px2 And ax2 > px2) ) Then

          If ( (ay1 < py1 And ay2 > py1) Or (ay1 < py2 And ay2 > py2) ) Then

        

         Player_Ammo_nextRemove = i1

         RemovePlayer_Ammo()

         next_alien_remove = i2

         remove_alien()

        

         toto= Shapes.AddImage(alien_expl1)

         Shapes.move(toto,ax1,ay1)

         Program.Delay(30)

         Shapes.Remove(toto)

         toto= Shapes.AddImage(alien_expl2)

         Shapes.move(toto,ax1,ay1)

         Program.Delay(30)

         Shapes.Remove(toto)

         score = score +100

        

          EndIf

         EndIf

        EndFor



     Endfor



  EndSub

'''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''



  Sub Collision_ep  

    px1 = Shapes.GetLeft(player)

    py1 = Shapes.GetTop(player)

    px2 = px1 + player_size

    py2 = py1 + player_size





    'Shapes.Move(Shapes.AddRectangle(px2-px1 ,py2-py1), px1, py1)

    For i2 = 1 to alien_Count

    

         ax1=Array.GetValue(alien_PosX,i2)

         ay1=Array.GetValue(alien_PosY,i2)



         ax2=ax1+alien_size

         ay2=ay1+alien_size

       

         If ( (px1 < ax1 And px2 > ax1) Or (px1 < ax2 And px2 > ax2) ) Then

          If ( (py1 < ay1 And py2 > ay1) Or (py1 < ay2 And py2 > ay2) ) Then

     

         next_alien_remove = i2

         remove_alien()

                toto= Shapes.AddImage(alien_expl1)

         Shapes.move(toto,ax1,ay1)

         Program.Delay(30)

         Shapes.Remove(toto)

         toto= Shapes.AddImage(player_expl)

         Shapes.move(toto,ax1,ay1)

         Program.Delay(300)

         Shapes.Remove(toto)

         Player_Lives = Player_Lives -1

         If (Player_Lives < 1) THEN

         

           pending = "True"

         EndIf

         If pending = "True" Then

         Goto gameover

         EndIf

        EndIf

       EndIf

     EndFor

    px1 = paddleX

    py1 = paddleY

    px2 = px1 + player_size

    py2 = py1 + player_size

      For i3 = 1 to Alien_AmmoCount

       

         ax1=Shapes.GetLeft(Array.GetValue(alien_Ammo,i3))

         ay1=Shapes.GetTop(Array.GetValue(alien_Ammo,i3))



         ax2=ax1+alien_ammo_size

         ay2=ay1+alien_ammo_size

       

         If ( (px1 < ax1 And px2 > ax1) Or (px1 < ax2 And px2 > ax2) ) Then

          If ( (py1 < ay1 And py2 > ay1) Or (py1 < ay2 And py2 > ay2) ) Then

      

         Alien_Ammo_nextRemove = i3

         RemoveAlien_Ammo()



         ' explosion at ax1, ay1

         toto= Shapes.AddImage(alien_expl1)

         Shapes.move(toto,paddleX+ player_size/2,paddleY+ player_size/2)

         Program.Delay(30)

         Shapes.Remove(toto)

         toto= Shapes.AddImage(player_expl)

         Shapes.move(toto,paddleX+ player_size/2,paddleY+ player_size/2)

         Program.Delay(300)

         Shapes.Remove(toto)

         Player_Lives = Player_Lives -1

         If (Player_Lives = 0) THEN

           Program.End()

         EndIf

        EndIf

       EndIf

      EndFor



  EndSub

