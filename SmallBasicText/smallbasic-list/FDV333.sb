EndSub

''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

Sub moveall

  

    incbx = 0

   incby =0

   GraphicsWindow.Title = gameTitle + score +" Lives:"+Player_Lives



   If (paddleX >(gameWidth-62) ) Then

     incx = incx - bgs

     incbx = incbx - bgs

     For i= 1 To planetcount

       incplanetx[i] = incplanetx[i] - bgs

     EndFor

          If (incx = -32 ) Then

            incx =0

          Endif

   Endif

   If (paddleX < 64 ) Then

     incx = incx + bgs

     incbx = incbx + bgs

          For i= 1 To planetcount

             incplanetx[i] = incplanetx[i] + bgs

          EndFor

          If (incx = 32 ) Then

            incx =0

          Endif

    Endif

    Shapes.Move(bgrnd,incx -32 ,incy - 32)



   For i= 1 To planetcount

      islx = posx[i]+incplanetx[i]

      isly = posy[i]+incplanety[i]

          Shapes.Move(Array.GetValue(planet_Array,i),islx,isly)

    EndFor



      incy = incy + bgs

      incby = incby+ bgs

    For i= 1 To planetcount

             incplanety[i] = incplanety[i] + bgs

    EndFor



    If (incy = 32) Then

      incy =0

    Endif



    For i= 1 To planetcount

       If ((posy[i]+incplanety[i]) > (gameHeight+15)) Then 

         R= Math.Round(Math.GetRandomNumber(nbrplanet))

         AA = Math.Remainder(TimePlay,6)

         'give new coordinates

         posx[i]= planetPos[AA][2]

         posy[i]= planetPos[AA][3]

         Shapes.move(Array.GetValue(planet_Array,R),posx[i],posy[i])

         incplanety[i]=0

         incplanetx[i]=0

       EndIf

    ENDFOR

     ' Move player shots

   For i = 1 To Player_AmmoCount

     Player_Ammox = Shapes.GetLeft(Array.GetValue(Player_Ammo, i)) +incbx

     Player_Ammoy = Shapes.GetTop(Array.GetValue(Player_Ammo, i)) - Player_Ammospeed

     Shapes.Move(Array.GetValue(Player_Ammo, i), Player_Ammox, Player_Ammoy)

     Array.SetValue(Player_AmmoAge, i, Array.GetValue(Player_AmmoAge, i) + 1)

   EndFor

     ' Move Alien shots

     For iea = 1 To Alien_AmmoCount

       dx =(Math.Sin((Array.GetValue(Alien_Ammo_Angle, iea) )) * Alien_Ammospeed)

       dy =(Math.Cos((Array.GetValue(Alien_Ammo_Angle, iea) ))* Alien_Ammospeed)

     Alien_Ammox = Shapes.GetLeft(Array.GetValue(Alien_Ammo, iea)) +dx+incbx

     Alien_Ammoy = Shapes.GetTop(Array.GetValue(Alien_Ammo, iea)) +dy+incby*0.1

     Shapes.Move(Array.GetValue(Alien_Ammo, iea), Alien_Ammox, Alien_Ammoy)

     Array.SetValue(Alien_AmmoAge, iea, Array.GetValue(Alien_AmmoAge, iea) + 1)

   EndFor





   ' move aliens

   For i=1 To alien_Count

    eNBR= Array.GetValue(alien_PathNBR,i)

   uu =Array.GetValue(alien_line,i)

   Time= Array.GetValue(alien_TimeLine, i)

   etl=AlienPath[eNBR][1][uu]  

   IF (Time=etl) Then  'rotate alien

     rr = AlienPath[eNBR][2][uu+1]

     Shapes.Rotate(Array.GetValue(alien_Array,i),rr)

   EndIf

   IF (Time > etl)  Then

     uu = uu+1

     Array.SetValue(alien_line,i,uu)    ' next line for alien move

   EndIf

   xx1=Array.GetValue(alien_PosX,i)

   yy1= Array.GetValue(alien_PosY,i)

     xx = xx1+AlienPath[eNBR][3][uu]+incbx

     yy = yy1+AlienPath[eNBR][4][uu]+incby*0.1

     ' Random alien fire

     If (Math.GetRandomNumber(Alien_Agresivity)=1) Then

       Alien_ShootX = xx1 + 16

       Alien_ShootY = yy1 + 4

       If (yy1 > 0 AND xx1 > 0 And yy1< gameHeight And xx1 < gameWidth) then

         'stops alien fire from outside the screen

         fire_Alien()

       Endif

     EndIf



     Shapes.Move(Array.GetValue(alien_Array,i),xx,yy)

     Array.SetValue(alien_PosX,i,xx)

     Array.SetValue(alien_PosY,i,yy)

     Array.SetValue(alien_TimeLine, i, Time + 1)

    If ((uu > AlienPath[eNBR][0][0]) And (Time > etl)) Then ' if last timelife remove the alien sprite 

       next_alien_remove = i

       remove_alien()

    endif

       Collision_pbe()

       Collision_ep()

EndFor

 



EndSub



'''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

Sub RemovePlayer_Ammo

Shapes.Remove(Array.GetValue(Player_Ammo, Player_Ammo_nextRemove))

For iz = Player_Ammo_nextRemove To Player_AmmoCount - 1

    Array.SetValue(Player_Ammo, iz, Array.GetValue(Player_Ammo, iz+1))

    Array.SetValue(Player_AmmoAge, iz, Array.GetValue(Player_AmmoAge, iz+1))

   EndFor

   Array.RemoveValue(Player_Ammo, Player_AmmoCount)

   Array.RemoveValue(Player_AmmoAge, Player_AmmoCount)

   Player_AmmoCount = Player_AmmoCount - 1

EndSub

'''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

Sub RemoveAlien_Ammo

Shapes.Remove(Array.GetValue(Alien_Ammo, Alien_Ammo_nextRemove))

For irea = Alien_Ammo_nextRemove To Alien_AmmoCount - 1

    Array.SetValue(Alien_Ammo, irea, Array.GetValue(Alien_Ammo, irea+1))

    Array.SetValue(Alien_AmmoAge, irea, Array.GetValue(Alien_AmmoAge, irea+1))

    Array.SetValue(Alien_Ammo_Angle, irea, Array.GetValue(Alien_Ammo_Angle,irea+1))



   EndFor

   Array.RemoveValue(Alien_Ammo, Alien_AmmoCount)

   Array.RemoveValue(Alien_AmmoAge, Alien_AmmoCount)

   Array.RemoveValue(Alien_Ammo_Angle,Alien_AmmoCount)

   Alien_AmmoCount = Alien_AmmoCount - 1

EndSub

'''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

Sub fire

   ' Remove additional player shots

   While(Player_AmmoCount > Player_AmmoMax )

     Player_Ammo_nextRemove = 1

     RemovePlayer_Ammo()

   EndWhile



   ' Add the player shots



   Player_AmmoCount = Player_AmmoCount + 1

   Array.SetValue(Player_Ammo, Player_AmmoCount, Shapes.AddImage(bullet0))

   Shapes.Move(Array.GetValue(Player_Ammo, Player_AmmoCount), ShootX, ShootY)

EndSub

'''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

Sub fire_Alien

   ' Remove additional Alien shots

   While(Alien_AmmoCount > (Alien_AmmoMax - 1))

     Alien_Ammo_nextRemove = 1

     RemoveAlien_Ammo()

   EndWhile



   ' Add the Alien shots



   Alien_AmmoCount = Alien_AmmoCount + 1

   Array.SetValue(Alien_Ammo, Alien_AmmoCount, Shapes.AddImage(Alien_bullet))

   Array.SetValue(Alien_Ammo_Angle, Alien_AmmoCount,Math.ArcTan((paddleX- Alien_ShootX+player_size/2)/(paddleY-Alien_ShootY)))

   Shapes.Move(Array.GetValue(Alien_Ammo, Alien_AmmoCount), Alien_ShootX, Alien_ShootY)

EndSub



''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

'Check player shots age

Sub AgePlayer_Ammo

    While (Array.GetValue(Player_AmmoAge, 1) > Player_AmmoLife)

     Player_Ammo_nextRemove = 1

     RemovePlayer_Ammo()

   EndWhile

 EndSub

 ''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

'Check alien ammo age

Sub AgeAlien_Ammo

   While (Array.GetValue(Alien_AmmoAge, 1) > Alien_AmmoLife)

     Alien_Ammo_nextRemove = 1

     RemoveAlien_Ammo()

   EndWhile

EndSub

''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

Sub remove_alien

    Shapes.remove(Array.GetValue(alien_Array,next_alien_remove))

      For ii = next_alien_remove To alien_Count - 1

     Array.SetValue(alien_Array, ii, Array.GetValue(alien_Array, ii+1))

     Array.SetValue(alien_line, ii, Array.GetValue(alien_line, ii+1))

     Array.SetValue(alien_PosX, ii, Array.GetValue(alien_PosX, ii+1))

     Array.SetValue(alien_PosY, ii, Array.GetValue(alien_PosY, ii+1))

     Array.SetValue(alien_TimeLine, ii, Array.GetValue(alien_TimeLine, ii+1))

     Array.SetValue(alien_PathNBR, ii , Array.GetValue(alien_PathNBR, ii+1))

    EndFor

     Array.RemoveValue(alien_Array, alien_Count)

     Array.RemoveValue(alien_line, alien_Count)

     Array.RemoveValue(alien_PosX, alien_Count)

     Array.RemoveValue(alien_PosY, alien_Count)

     Array.RemoveValue(alien_TimeLine, alien_Count)

     Array.RemoveValue(alien_PathNBR,alien_Count)

     alien_Count = alien_Count - 1

   EndSub

   

''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

Sub create_aliens_left



     TimeLine1 =0

     For i=1 To alien_Nbr

       alien_Count= alien_Count + 1

       Array.SetValue(alien_PathNBR,alien_Count,pathNBR)

       If (alien_type = 2) Then

         Array.SetValue(alien_Array, alien_Count,Shapes.AddImage(alien2))

         Else

           Array.SetValue(alien_Array, alien_Count,Shapes.AddImage(alien))

         EndIf

       Array.SetValue(alien_line,alien_Count,1)

       Array.SetValue(alien_PosX, alien_Count,alienPosX1)

       Array.SetValue(alien_PosY, alien_Count,alienPosY1)

       Array.SetValue(alien_TimeLine, alien_Count, TimeLine1)



       alienPosX1 = alienPosX1 - 64 'distance between aliens

       TimeLine1 = TimeLine1 - 64 / Alien_Speed

    EndFor

    For i=(alien_Count-alien_Nbr+1) To alien_Count

      xxx=Array.GetValue(alien_PosX,i)

      yyy= Array.GetValue(alien_PosY,i)

    Shapes.Move(Array.GetValue(alien_Array,i),xxx,yyy)

    EndFor

  EndSub

  

  ''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

  

  Sub create_aliens_right



     TimeLine1 =0

     For i=1 To alien_Nbr

       alien_Count= alien_Count + 1

       Array.SetValue(alien_PathNBR,alien_Count,pathNBR)

       Array.SetValue(alien_Array, alien_Count,Shapes.AddImage(alien))

       Array.SetValue(alien_line,alien_Count,1)

       Array.SetValue(alien_PosX, alien_Count,alienPosX1)

       Array.SetValue(alien_PosY, alien_Count,alienPosY1)

       Array.SetValue(alien_TimeLine, alien_Count, TimeLine1)



       alienPosX1 = alienPosX1 + 64 'distance between aliens

       TimeLine1 = TimeLine1 - 64 / Alien_Speed

    EndFor

    For i=(alien_Count-alien_Nbr+1) To alien_Count

      xxx=Array.GetValue(alien_PosX,i)

      yyy= Array.GetValue(alien_PosY,i)

