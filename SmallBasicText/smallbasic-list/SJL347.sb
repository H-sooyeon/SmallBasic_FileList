Sub nul0

  

  If x=0 Then

    x=mod

    EndIf

  

  If x<10 Then

    TextWindow.Write("  "+x)

  Else

    TextWindow.Write(" "+x)

  EndIf

EndSub





Sub nul1

  

  If x=0 Then

    x=mod

    EndIf

  

  If x<10 Then

    TextWindow.Write("   "+x)

  Else

    TextWindow.Write("  "+x)

  EndIf

EndSub

'-----------------

Sub nul

  

  If x<10 Then

    TextWindow.Write(" ")

  EndIf

EndSub

