Sub el4

  

   If prim=1  Then

    feu= Math.Remainder((feu+el),mod)

  EndIf

  

  If prim=3  Then

    air= Math.Remainder((air+el),mod)

  EndIf

  

  If prim=2  Then

    eau= Math.Remainder((eau+el),mod)

  EndIf

  

  If prim=0  Then

    ter= Math.Remainder((ter+el),mod)

  EndIf

  

EndSub

