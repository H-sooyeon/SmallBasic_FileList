Sub PlayBackLog

  Speed = .5

  For x = 1 to LoopCount

    'ID, Date, Time, Terminal, Berth, Route, Vessel, Step1, Step2, Step3, Activity

    'Depart = 1  'Arrive = 2  'UnLoadBegin = 4  'UnLoadEnd = 5  'LoadBegin = 6  'LoadEnd = 7  'Safety = 8

    

    If Berth[x] = 1 Then

      If Step1[x] = 1 then  'Depart = 1  

        Vessel1Departure()

      ElseIf Step1[x] = 2 then  'Arrive = 2

        Vessel1Arrive()

        Berth1RampDown()

        '   Program.Delay(5000)

      ElseIf Step1[x] = 3 then

      ElseIf Step1[x] = 4 then  'UnLoadBegin = 4

        Berth1Unloading()

        '   Program.Delay(10000)

      ElseIf Step1[x] = 5 then  'UnLoadEnd = 5

        Berth1UnloadingStop()

        '   Program.Delay(10000)

      ElseIf Step1[x] = 6 then  'LoadBegin = 6

        Berth1Loading()

        '   Program.Delay(10000)

      ElseIf Step1[x] = 7 then  'LoadEnd = 7

        Berth1LoadingStop()

        '   Program.Delay(5000)

      ElseIf Step1[x] = 8 then  'Safety = 8

        Berth1RampUp()

        '   Program.Delay(2000)

      EndIf

      

    ElseIf Berth[x] = 2 then

      If Step1[x] = 1 then  'Depart = 1  

        Vessel2Departure()

      ElseIf Step1[x] = 2 then  'Arrive = 2

        Vessel2Arrive()

        Berth2RampDown()

        '   Program.Delay(5000)

      ElseIf Step1[x] = 3 then

      ElseIf Step1[x] = 4 then  'UnLoadBegin = 4

        Berth2Unloading()

        '   Program.Delay(10000)

      ElseIf Step1[x] = 5 then  'UnLoadEnd = 5

        Berth2UnloadingEnd()

        '   Program.Delay(10000)

      ElseIf Step1[x] = 6 then  'LoadBegin = 6

        Berth2Loading()

        '   Program.Delay(10000)

      ElseIf Step1[x] = 7 then  'LoadEnd = 7

        Berth2LoadingEnd()

        '   Program.Delay(5000)

      ElseIf Step1[x] = 8 then  'Safety = 8

        Berth2RampUp()

        '   Program.Delay(2000)

      EndIf

    elseif Berth[x] = 3 then

      If Step1[x] = 1 then  'Depart = 1  

        Vessel3Departure()

      ElseIf Step1[x] = 2 then  'Arrive = 2

        Vessel3Arrive()

        Berth3RampDown()

        '   Program.Delay(5000)

      ElseIf Step1[x] = 3 then

      ElseIf Step1[x] = 4 then  'UnLoadBegin = 4

        Berth3Unloading()

        '   Program.Delay(10000)

      ElseIf Step1[x] = 5 then  'UnLoadEnd = 5

        Berth3UnloadingEnd()

        '   Program.Delay(10000)

      ElseIf Step1[x] = 6 then  'LoadBegin = 6

        Berth3Loading()

        '   Program.Delay(10000)

      ElseIf Step1[x] = 7 then  'LoadEnd = 7

        Berth3LoadingEnd()

        '   Program.Delay(5000)

      ElseIf Step1[x] = 8 then  'Safety = 8

        Berth3RampUp()

        '   Program.Delay(2000)

      EndIf

    EndIf

  endfor

endsub

